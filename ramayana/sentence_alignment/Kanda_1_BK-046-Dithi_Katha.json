{
  "fragments": [
    {
      "begin": "0.000",
      "children": [],
      "end": "3.440",
      "id": "f000001",
      "language": "hi",
      "lines": [
        "हतेषु तेषु पुत्रेषु दितिः परमदुःखिता"
      ]
    },
    {
      "begin": "3.440",
      "children": [],
      "end": "8.560",
      "id": "f000002",
      "language": "hi",
      "lines": [
        "मारीचं काश्यपं राम भर्त्तारमिदमब्रवीत्"
      ]
    },
    {
      "begin": "8.560",
      "children": [],
      "end": "18.560",
      "id": "f000003",
      "language": "hi",
      "lines": [
        "हतपुत्रास्मि भगवंस्तव पुत्रैर्महाबलैः"
      ]
    },
    {
      "begin": "18.560",
      "children": [],
      "end": "23.040",
      "id": "f000004",
      "language": "hi",
      "lines": [
        "शक्रहन्तारमिच्छामि पुत्रं दीर्घतपोर्जितम्"
      ]
    },
    {
      "begin": "23.040",
      "children": [],
      "end": "27.520",
      "id": "f000005",
      "language": "hi",
      "lines": [
        "साहं तपश्चरिष्यामि गर्भं मे दातुमर्हसि"
      ]
    },
    {
      "begin": "27.520",
      "children": [],
      "end": "31.840",
      "id": "f000006",
      "language": "hi",
      "lines": [
        "ईश्वरं शक्रहन्तारं त्वमनुज्ञातुमर्हसि"
      ]
    },
    {
      "begin": "31.840",
      "children": [],
      "end": "36.680",
      "id": "f000007",
      "language": "hi",
      "lines": [
        "तस्यास्तद्वचनं श्रुत्वा मारीचः काश्यपस्तदा"
      ]
    },
    {
      "begin": "36.680",
      "children": [],
      "end": "41.080",
      "id": "f000008",
      "language": "hi",
      "lines": [
        "प्रत्युवाच महातेजा दितिं परमदुःखिताम्"
      ]
    },
    {
      "begin": "41.080",
      "children": [],
      "end": "44.960",
      "id": "f000009",
      "language": "hi",
      "lines": [
        "एवं भवतु भद्रं ते शुचिर्भव तपोधने"
      ]
    },
    {
      "begin": "44.960",
      "children": [],
      "end": "49.000",
      "id": "f000010",
      "language": "hi",
      "lines": [
        "जनयिष्यसि पुत्रं त्वं शक्रहन्तारमाहवे"
      ]
    },
    {
      "begin": "49.000",
      "children": [],
      "end": "53.080",
      "id": "f000011",
      "language": "hi",
      "lines": [
        "पूर्णे वर्षसहस्रे तु शुचिर्यदि भविष्यसि"
      ]
    },
    {
      "begin": "53.080",
      "children": [],
      "end": "57.640",
      "id": "f000012",
      "language": "hi",
      "lines": [
        "पुत्रं त्रैलोक्यभर्तारं मत्तस्त्वं जनयिष्यसि"
      ]
    },
    {
      "begin": "57.640",
      "children": [],
      "end": "62.720",
      "id": "f000013",
      "language": "hi",
      "lines": [
        "एवमुक्त्वा महातेजाः पाणिना स ममार्ज ताम्"
      ]
    },
    {
      "begin": "62.720",
      "children": [],
      "end": "66.880",
      "id": "f000014",
      "language": "hi",
      "lines": [
        "समालभ्य ततः स्वस्तीत्युक्त्वा स तपसे ययौ"
      ]
    },
    {
      "begin": "66.880",
      "children": [],
      "end": "70.920",
      "id": "f000015",
      "language": "hi",
      "lines": [
        "गते तस्मिन्नरश्रेष्ठ दितिः परमहर्षिता"
      ]
    },
    {
      "begin": "70.920",
      "children": [],
      "end": "74.680",
      "id": "f000016",
      "language": "hi",
      "lines": [
        "कुशप्लवनमासाद्य तपस्तेपे सुदारुणम्"
      ]
    },
    {
      "begin": "74.680",
      "children": [],
      "end": "79.840",
      "id": "f000017",
      "language": "hi",
      "lines": [
        "तपस्तस्यां हि कुर्वन्त्यां परिचर्यां चकार ह"
      ]
    },
    {
      "begin": "79.840",
      "children": [],
      "end": "84.160",
      "id": "f000018",
      "language": "hi",
      "lines": [
        "सहस्राक्षो नरश्रेष्ठ परया गुणसम्पदा"
      ]
    },
    {
      "begin": "84.160",
      "children": [],
      "end": "88.840",
      "id": "f000019",
      "language": "hi",
      "lines": [
        "अग्निं कुशान् काष्ठमपः फलं मूलं तथैव च"
      ]
    },
    {
      "begin": "88.840",
      "children": [],
      "end": "93.160",
      "id": "f000020",
      "language": "hi",
      "lines": [
        "न्यवेदयत् सहस्राक्षो यच्चान्यदपि काङ्क्षितम्"
      ]
    },
    {
      "begin": "93.160",
      "children": [],
      "end": "98.080",
      "id": "f000021",
      "language": "hi",
      "lines": [
        "गात्रसंवहनैश्चैव श्रमापनयनैस्तथा"
      ]
    },
    {
      "begin": "98.080",
      "children": [],
      "end": "102.320",
      "id": "f000022",
      "language": "hi",
      "lines": [
        "शक्रः सर्वेषु कालेषु दितिं परिचचार ह"
      ]
    },
    {
      "begin": "102.320",
      "children": [],
      "end": "106.760",
      "id": "f000023",
      "language": "hi",
      "lines": [
        "अथ वर्षसहस्रे तु दशोने रघुनन्दन"
      ]
    },
    {
      "begin": "106.760",
      "children": [],
      "end": "110.880",
      "id": "f000024",
      "language": "hi",
      "lines": [
        "दितिः परमसम्प्रीता सहस्राक्षमथाब्रवीत्"
      ]
    },
    {
      "begin": "110.880",
      "children": [],
      "end": "115.520",
      "id": "f000025",
      "language": "hi",
      "lines": [
        "तपश्चरन्त्या वर्षाणि दश वीर्यवतां वर"
      ]
    },
    {
      "begin": "115.520",
      "children": [],
      "end": "120.000",
      "id": "f000026",
      "language": "hi",
      "lines": [
        "अवशिष्टानि भद्रं ते भ्रातरं द्रक्ष्यसे ततः"
      ]
    },
    {
      "begin": "120.000",
      "children": [],
      "end": "124.120",
      "id": "f000027",
      "language": "hi",
      "lines": [
        "तमहं त्वत्कृते पुत्रं समाधास्ये जयोत्सुकम्"
      ]
    },
    {
      "begin": "124.120",
      "children": [],
      "end": "128.560",
      "id": "f000028",
      "language": "hi",
      "lines": [
        "त्रैलोक्यविजयं पुत्रं सह भोक्ष्यसि विज्वरः"
      ]
    },
    {
      "begin": "128.560",
      "children": [],
      "end": "133.040",
      "id": "f000029",
      "language": "hi",
      "lines": [
        "याचितेन सुरश्रेष्ठ पित्रा तव महात्मना"
      ]
    },
    {
      "begin": "133.040",
      "children": [],
      "end": "136.360",
      "id": "f000030",
      "language": "hi",
      "lines": [
        "वरो वर्षसहस्रान्ते मम दत्तः सुतं प्रति"
      ]
    },
    {
      "begin": "136.360",
      "children": [],
      "end": "142.120",
      "id": "f000031",
      "language": "hi",
      "lines": [
        "एवमुक्त्वा दितिः शक्रं प्राप्ते मध्यं दिवाकरे"
      ]
    },
    {
      "begin": "142.120",
      "children": [],
      "end": "146.880",
      "id": "f000032",
      "language": "hi",
      "lines": [
        "निद्रयापहृता देवी पादौ कृत्वाथ शीर्षतः"
      ]
    },
    {
      "begin": "146.880",
      "children": [],
      "end": "151.560",
      "id": "f000033",
      "language": "hi",
      "lines": [
        "दृष्ट्वा तामशुचिं शक्रः पादतः कृतमूर्द्धजाम्"
      ]
    },
    {
      "begin": "151.560",
      "children": [],
      "end": "155.800",
      "id": "f000034",
      "language": "hi",
      "lines": [
        "शिरःस्थाने कृतौ पादौ जहास च मुमोद च"
      ]
    },
    {
      "begin": "155.800",
      "children": [],
      "end": "159.240",
      "id": "f000035",
      "language": "hi",
      "lines": [
        "तस्याः शरीरविवरं विवेश च पुरन्दरः"
      ]
    },
    {
      "begin": "159.240",
      "children": [],
      "end": "164.600",
      "id": "f000036",
      "language": "hi",
      "lines": [
        "गर्भं च सप्तधा राम बिभेद परमात्मवान्"
      ]
    },
    {
      "begin": "164.600",
      "children": [],
      "end": "168.840",
      "id": "f000037",
      "language": "hi",
      "lines": [
        "भिद्यमानस्ततो गर्भो वज्रेण शतपर्वणा"
      ]
    },
    {
      "begin": "168.840",
      "children": [],
      "end": "172.480",
      "id": "f000038",
      "language": "hi",
      "lines": [
        "रुरोद सुस्वरं राम ततो दितिरबुध्यत"
      ]
    },
    {
      "begin": "172.480",
      "children": [],
      "end": "177.000",
      "id": "f000039",
      "language": "hi",
      "lines": [
        "मा रुदो मा रुदश्चेति गर्भं शक्रोऽभ्यभाषत"
      ]
    },
    {
      "begin": "177.000",
      "children": [],
      "end": "179.760",
      "id": "f000040",
      "language": "hi",
      "lines": [
        "बिभेद च महातेजा रुदन्तमपि वासवः"
      ]
    },
    {
      "begin": "179.760",
      "children": [],
      "end": "185.080",
      "id": "f000041",
      "language": "hi",
      "lines": [
        "न हन्तव्यो न हन्तव्य इत्येवं दितिरब्रवीत्"
      ]
    },
    {
      "begin": "185.080",
      "children": [],
      "end": "189.240",
      "id": "f000042",
      "language": "hi",
      "lines": [
        "निष्पपात ततः शक्रो मातुर्वचनगौरवात्"
      ]
    },
    {
      "begin": "189.240",
      "children": [],
      "end": "195.080",
      "id": "f000043",
      "language": "hi",
      "lines": [
        "प्राञ्जलिर्वज्रसहितो दितिं शक्रोऽभ्यभाषत"
      ]
    },
    {
      "begin": "195.080",
      "children": [],
      "end": "208.400",
      "id": "f000044",
      "language": "hi",
      "lines": [
        "अशुचिर्देवि सुप्तासि पादयोः कृतमूर्द्धजा"
      ]
    },
    {
      "begin": "208.400",
      "children": [],
      "end": "212.680",
      "id": "f000045",
      "language": "hi",
      "lines": [
        "तदन्तरमहं लब्ध्वा शक्रहन्तारमाहवे"
      ]
    },
    {
      "begin": "212.680",
      "children": [],
      "end": "221.840",
      "id": "f000046",
      "language": "hi",
      "lines": [
        "अभिदं सप्तधा देवि तन्मे त्वं क्षन्तुमर्हसि"
      ]
    }
  ]
}