{
  "fragments": [
    {
      "begin": "0.000",
      "children": [],
      "end": "9.800",
      "id": "f000001",
      "language": "hi",
      "lines": [
        "जनकेन समादिष्टा दूतास्ते क्लान्तवाहनाः"
      ]
    },
    {
      "begin": "9.800",
      "children": [],
      "end": "17.280",
      "id": "f000002",
      "language": "hi",
      "lines": [
        "त्रिरात्रमुषिता मार्गे तेऽयोध्यां प्राविशन् पुरीम्"
      ]
    },
    {
      "begin": "17.280",
      "children": [],
      "end": "21.920",
      "id": "f000003",
      "language": "hi",
      "lines": [
        "ते राजवचनाद्दूता राजवेश्म प्रवेशिताः"
      ]
    },
    {
      "begin": "21.920",
      "children": [],
      "end": "25.600",
      "id": "f000004",
      "language": "hi",
      "lines": [
        "ददृशुर्देवसङ्काशं वृद्धं दशरथं नृपम्"
      ]
    },
    {
      "begin": "25.600",
      "children": [],
      "end": "30.600",
      "id": "f000005",
      "language": "hi",
      "lines": [
        "बद्धाञ्जलिपुटाः सर्वे दूताविगतसाध्वसाः"
      ]
    },
    {
      "begin": "30.600",
      "children": [],
      "end": "34.400",
      "id": "f000006",
      "language": "hi",
      "lines": [
        "राजानं प्रयता वाक्यमब्रुवन् मधुराक्षरम्"
      ]
    },
    {
      "begin": "34.400",
      "children": [],
      "end": "39.120",
      "id": "f000007",
      "language": "hi",
      "lines": [
        "मैथिलो जनको राजा साग्निहोत्रपुरस्कृतम्"
      ]
    },
    {
      "begin": "39.120",
      "children": [],
      "end": "43.680",
      "id": "f000008",
      "language": "hi",
      "lines": [
        "कुशलं चाव्ययं चैव सोपाध्यायपुरोहितम्"
      ]
    },
    {
      "begin": "43.680",
      "children": [],
      "end": "46.840",
      "id": "f000009",
      "language": "hi",
      "lines": [
        "मुहुर्मुहुर्मधुरया स्नेहसंयुक्तया गिरा"
      ]
    },
    {
      "begin": "46.840",
      "children": [],
      "end": "51.000",
      "id": "f000010",
      "language": "hi",
      "lines": [
        "जनकस्त्वां महाराजा पृच्छते सपुरःसरम्"
      ]
    },
    {
      "begin": "51.000",
      "children": [],
      "end": "55.640",
      "id": "f000011",
      "language": "hi",
      "lines": [
        "पृष्ट्वा कुशलमव्यग्रं वैदेहो मिथिलाधिपः"
      ]
    },
    {
      "begin": "55.640",
      "children": [],
      "end": "59.640",
      "id": "f000012",
      "language": "hi",
      "lines": [
        "कौशिकानुमते वाक्यं भवन्तमिदमब्रवीत्"
      ]
    },
    {
      "begin": "59.640",
      "children": [],
      "end": "63.680",
      "id": "f000013",
      "language": "hi",
      "lines": [
        "पूर्वं प्रतिज्ञा विदिता वीर्यशुल्का ममात्मजा"
      ]
    },
    {
      "begin": "63.680",
      "children": [],
      "end": "69.080",
      "id": "f000014",
      "language": "hi",
      "lines": [
        "राजानश्च कृतामर्षा निर्वीर्या विमुखीकृताः"
      ]
    },
    {
      "begin": "69.080",
      "children": [],
      "end": "72.280",
      "id": "f000015",
      "language": "hi",
      "lines": [
        "सेयं मम सुता राजन् विश्वामित्रपुरस्सरैः"
      ]
    },
    {
      "begin": "72.280",
      "children": [],
      "end": "77.480",
      "id": "f000016",
      "language": "hi",
      "lines": [
        "यदृच्छयागतैर्वीरैर्निर्जिता तव पुत्रकैः"
      ]
    },
    {
      "begin": "77.480",
      "children": [],
      "end": "82.320",
      "id": "f000017",
      "language": "hi",
      "lines": [
        "तच्च राजन् धनुर्दिव्यं मध्ये भग्नं महात्मना"
      ]
    },
    {
      "begin": "82.320",
      "children": [],
      "end": "86.040",
      "id": "f000018",
      "language": "hi",
      "lines": [
        "रामेण हि महाराज महत्यां जनसंसदि"
      ]
    },
    {
      "begin": "86.040",
      "children": [],
      "end": "90.600",
      "id": "f000019",
      "language": "hi",
      "lines": [
        "अस्मै देया मया सीता वीर्यशुल्का महात्मने"
      ]
    },
    {
      "begin": "90.600",
      "children": [],
      "end": "94.720",
      "id": "f000020",
      "language": "hi",
      "lines": [
        "प्रतिज्ञां कर्तुमिच्छामि तदनुज्ञातुमर्हसि"
      ]
    },
    {
      "begin": "94.720",
      "children": [],
      "end": "99.120",
      "id": "f000021",
      "language": "hi",
      "lines": [
        "सोपाध्यायो महाराज पुरोहितपुरस्सरः"
      ]
    },
    {
      "begin": "99.120",
      "children": [],
      "end": "103.600",
      "id": "f000022",
      "language": "hi",
      "lines": [
        "शीघ्रमागच्छ भद्रं ते द्रष्टुमर्हसि राघवौ"
      ]
    },
    {
      "begin": "103.600",
      "children": [],
      "end": "107.480",
      "id": "f000023",
      "language": "hi",
      "lines": [
        "प्रीतिं च मम राजेन्द्र निर्वर्तयितुमर्हसि"
      ]
    },
    {
      "begin": "107.480",
      "children": [],
      "end": "111.800",
      "id": "f000024",
      "language": "hi",
      "lines": [
        "पुत्रयोरुभयोरेव प्रीतिं त्वमपि लप्स्यसे"
      ]
    },
    {
      "begin": "111.800",
      "children": [],
      "end": "115.920",
      "id": "f000025",
      "language": "hi",
      "lines": [
        "एवं विदेहाधिपतिर्मधुरं वाक्यमब्रवीत्"
      ]
    },
    {
      "begin": "115.920",
      "children": [],
      "end": "120.760",
      "id": "f000026",
      "language": "hi",
      "lines": [
        "विश्वामित्राभ्यनुज्ञातः शतानन्दमते स्थितः"
      ]
    },
    {
      "begin": "120.760",
      "children": [],
      "end": "125.040",
      "id": "f000027",
      "language": "hi",
      "lines": [
        "दूतवाक्यं तु तच्छ्रुत्वा राजा परमहर्षितः"
      ]
    },
    {
      "begin": "125.040",
      "children": [],
      "end": "129.200",
      "id": "f000028",
      "language": "hi",
      "lines": [
        "वसिष्ठं वामदेवं च मन्त्रिणोऽन्यांश्च सोऽब्रवीत्"
      ]
    },
    {
      "begin": "129.200",
      "children": [],
      "end": "133.320",
      "id": "f000029",
      "language": "hi",
      "lines": [
        "गुप्तः कुशिकपुत्रेण कौसल्यानन्दवर्द्धनः"
      ]
    },
    {
      "begin": "133.320",
      "children": [],
      "end": "137.520",
      "id": "f000030",
      "language": "hi",
      "lines": [
        "लक्ष्मणेन सह भ्रात्रा विदेहेषु वसत्यसौ"
      ]
    },
    {
      "begin": "137.520",
      "children": [],
      "end": "141.880",
      "id": "f000031",
      "language": "hi",
      "lines": [
        "दृष्टवीर्यस्तु काकुत्स्थो जनकेन महात्मना"
      ]
    },
    {
      "begin": "141.880",
      "children": [],
      "end": "146.200",
      "id": "f000032",
      "language": "hi",
      "lines": [
        "सम्प्रदानं सुतायास्तु राघवे कर्त्तुमिच्छति"
      ]
    },
    {
      "begin": "146.200",
      "children": [],
      "end": "150.320",
      "id": "f000033",
      "language": "hi",
      "lines": [
        "यदि वो रोचते वृत्तं जनकस्य महात्मनः"
      ]
    },
    {
      "begin": "150.320",
      "children": [],
      "end": "155.240",
      "id": "f000034",
      "language": "hi",
      "lines": [
        "पुरीं गच्छामहे शीघ्रं माभूत्कालस्य पर्ययः"
      ]
    },
    {
      "begin": "155.240",
      "children": [],
      "end": "159.440",
      "id": "f000035",
      "language": "hi",
      "lines": [
        "मन्त्रिणो बाढमित्याहुः सह सर्वैर्महर्षिभिः"
      ]
    },
    {
      "begin": "159.440",
      "children": [],
      "end": "172.280",
      "id": "f000036",
      "language": "hi",
      "lines": [
        "सुप्रीतश्चाब्रवीद्राजा श्वो यात्रेति स मन्त्रिणः"
      ]
    },
    {
      "begin": "172.280",
      "children": [],
      "end": "180.640",
      "id": "f000037",
      "language": "hi",
      "lines": [
        "मन्त्रिणस्तां नरेन्द्रस्य रात्रिं परमसत्कृताः"
      ]
    },
    {
      "begin": "180.640",
      "children": [],
      "end": "190.480",
      "id": "f000038",
      "language": "hi",
      "lines": [
        "ऊषुस्ते मुदिताः सर्वे गुणैः सर्वैस्समन्विताः"
      ]
    }
  ]
}