{
  "fragments": [
    {
      "begin": "0.000",
      "children": [],
      "end": "11.280",
      "id": "f000001",
      "language": "hi",
      "lines": [
        "अपश्यंस्तु ततस्तत्र पितरं पितुरालये"
      ]
    },
    {
      "begin": "11.280",
      "children": [],
      "end": "16.200",
      "id": "f000002",
      "language": "hi",
      "lines": [
        "जगाम भरतो द्रष्टुं मातरं मातुरालये"
      ]
    },
    {
      "begin": "16.200",
      "children": [],
      "end": "21.480",
      "id": "f000003",
      "language": "hi",
      "lines": [
        "अनुप्राप्तं तु तं दृष्ट्वा कैकेयी प्रोषितं सुतम्"
      ]
    },
    {
      "begin": "21.480",
      "children": [],
      "end": "26.160",
      "id": "f000004",
      "language": "hi",
      "lines": [
        "उत्पपात तदा हृष्टा त्यक्त्वा सौवर्णमासनम्"
      ]
    },
    {
      "begin": "26.160",
      "children": [],
      "end": "31.560",
      "id": "f000005",
      "language": "hi",
      "lines": [
        "स प्रविश्यैव धर्मात्मा स्वगृहं श्रीविवर्जितम्"
      ]
    },
    {
      "begin": "31.560",
      "children": [],
      "end": "36.360",
      "id": "f000006",
      "language": "hi",
      "lines": [
        "भरतः प्रतिजग्राह जनन्याश्चरणौ शुभौ"
      ]
    },
    {
      "begin": "36.360",
      "children": [],
      "end": "41.360",
      "id": "f000007",
      "language": "hi",
      "lines": [
        "सा तं मूर्द्धन्युपाघ्राया परिष्वज्य यशस्विनम्"
      ]
    },
    {
      "begin": "41.360",
      "children": [],
      "end": "46.120",
      "id": "f000008",
      "language": "hi",
      "lines": [
        "अङ्के भरतमारोप्य प्रष्टुं समुपचक्रमे"
      ]
    },
    {
      "begin": "46.120",
      "children": [],
      "end": "53.400",
      "id": "f000009",
      "language": "hi",
      "lines": [
        "अपि नाध्वश्रमः शीघ्रं रथेनापततस्तव\\*"
      ]
    },
    {
      "begin": "53.400",
      "children": [],
      "end": "55.880",
      "id": "f000010",
      "language": "hi",
      "lines": [
        "आर्यकः मातामहः"
      ]
    },
    {
      "begin": "55.880",
      "children": [],
      "end": "59.720",
      "id": "f000011",
      "language": "hi",
      "lines": [
        "आर्यकस्ते सुकुशली युधाजिन्मातुलस्तव"
      ]
    },
    {
      "begin": "59.720",
      "children": [],
      "end": "65.240",
      "id": "f000012",
      "language": "hi",
      "lines": [
        "प्रवासाच्च सुखं पुत्र सर्वं मे वक्तुमर्हसि"
      ]
    },
    {
      "begin": "65.240",
      "children": [],
      "end": "70.520",
      "id": "f000013",
      "language": "hi",
      "lines": [
        "एवं पृष्टस्तु कैकेय्या प्रियं पार्थिवनन्दनः"
      ]
    },
    {
      "begin": "70.520",
      "children": [],
      "end": "74.520",
      "id": "f000014",
      "language": "hi",
      "lines": [
        "आचष्ट भरतः सर्वं मात्रे राजीवलोचनः"
      ]
    },
    {
      "begin": "74.520",
      "children": [],
      "end": "80.160",
      "id": "f000015",
      "language": "hi",
      "lines": [
        "अद्य मे सप्तमी रात्रिश्च्युतस्यार्यकवेश्मनः"
      ]
    },
    {
      "begin": "80.160",
      "children": [],
      "end": "86.160",
      "id": "f000016",
      "language": "hi",
      "lines": [
        "अम्बायाः कुशली तातो युधाजिन्मातुलश्च मे"
      ]
    },
    {
      "begin": "86.160",
      "children": [],
      "end": "90.680",
      "id": "f000017",
      "language": "hi",
      "lines": [
        "यन्मे धनं च रत्नं च ददौ राजा परन्तपः"
      ]
    },
    {
      "begin": "90.680",
      "children": [],
      "end": "96.120",
      "id": "f000018",
      "language": "hi",
      "lines": [
        "परिश्रान्तं पथ्यभवत् ततोऽहं पूर्वमागतः"
      ]
    },
    {
      "begin": "96.120",
      "children": [],
      "end": "101.520",
      "id": "f000019",
      "language": "hi",
      "lines": [
        "राजवाक्यहरैर्दूतैस्त्वर्यमाणोऽहमागतः"
      ]
    },
    {
      "begin": "101.520",
      "children": [],
      "end": "105.760",
      "id": "f000020",
      "language": "hi",
      "lines": [
        "यदहं प्रष्टुमिच्छामि तदम्बा वक्तुमर्हति"
      ]
    },
    {
      "begin": "105.760",
      "children": [],
      "end": "111.160",
      "id": "f000021",
      "language": "hi",
      "lines": [
        "शून्योऽयं शयनीयस्ते पर्यङ्को हेमभूषितः"
      ]
    },
    {
      "begin": "111.160",
      "children": [],
      "end": "115.560",
      "id": "f000022",
      "language": "hi",
      "lines": [
        "न चायमिक्ष्वाकुजनः प्रहृष्टः प्रतिभाति मा"
      ]
    },
    {
      "begin": "115.560",
      "children": [],
      "end": "120.160",
      "id": "f000023",
      "language": "hi",
      "lines": [
        "राजा भवति भूयिष्ठमिहाम्बाया निवेशने"
      ]
    },
    {
      "begin": "120.160",
      "children": [],
      "end": "125.160",
      "id": "f000024",
      "language": "hi",
      "lines": [
        "तमहं नाद्य पश्यामि द्रष्टुमिच्छन्निहागतः"
      ]
    },
    {
      "begin": "125.160",
      "children": [],
      "end": "130.400",
      "id": "f000025",
      "language": "hi",
      "lines": [
        "पितुर्ग्रहीष्ये चरणौ तं ममाख्याहि पृच्छतः"
      ]
    },
    {
      "begin": "130.400",
      "children": [],
      "end": "136.720",
      "id": "f000026",
      "language": "hi",
      "lines": [
        "आहोस्विदम्ब ज्येष्ठायाः कौसल्याया निवेशने"
      ]
    },
    {
      "begin": "136.720",
      "children": [],
      "end": "140.760",
      "id": "f000027",
      "language": "hi",
      "lines": [
        "तं प्रत्युवाच कैकेयी प्रियवद् घोरमप्रियम्"
      ]
    },
    {
      "begin": "140.760",
      "children": [],
      "end": "146.280",
      "id": "f000028",
      "language": "hi",
      "lines": [
        "अजानन्तं प्रजानन्ती राज्यलोभेन मोहिता"
      ]
    },
    {
      "begin": "146.280",
      "children": [],
      "end": "152.000",
      "id": "f000029",
      "language": "hi",
      "lines": [
        "या गतिः सर्वभूतानां तां गतिं ते पिता गतः"
      ]
    },
    {
      "begin": "152.000",
      "children": [],
      "end": "156.920",
      "id": "f000030",
      "language": "hi",
      "lines": [
        "राजा महात्मा तेजस्वी यावजूकः सतां गतिः"
      ]
    },
    {
      "begin": "156.920",
      "children": [],
      "end": "161.760",
      "id": "f000031",
      "language": "hi",
      "lines": [
        "तच्छ्रुत्वा भरतो वाक्यं धर्माभिजनवाञ्छुचिः"
      ]
    },
    {
      "begin": "161.760",
      "children": [],
      "end": "165.120",
      "id": "f000032",
      "language": "hi",
      "lines": [
        "पपात सहसा भूमौ पितृशोकबलार्दितः"
      ]
    },
    {
      "begin": "165.120",
      "children": [],
      "end": "171.560",
      "id": "f000033",
      "language": "hi",
      "lines": [
        "हा हतोस्मीति कृपणां दीनां वाचमुदीरयन्"
      ]
    },
    {
      "begin": "171.560",
      "children": [],
      "end": "176.040",
      "id": "f000034",
      "language": "hi",
      "lines": [
        "निपपात महाबाहुर्बाहू विक्षिप्य वीर्यवान्"
      ]
    },
    {
      "begin": "176.040",
      "children": [],
      "end": "180.880",
      "id": "f000035",
      "language": "hi",
      "lines": [
        "ततः शोकेन संवीतः पितुर्मरणदुःखितः"
      ]
    },
    {
      "begin": "180.880",
      "children": [],
      "end": "185.880",
      "id": "f000036",
      "language": "hi",
      "lines": [
        "विललाप महातेजा भ्रान्ताकुलितचेतनः"
      ]
    },
    {
      "begin": "185.880",
      "children": [],
      "end": "190.160",
      "id": "f000037",
      "language": "hi",
      "lines": [
        "एतत् सुरुचिरं भाति पितुर्मे शयनं पुरा"
      ]
    },
    {
      "begin": "190.160",
      "children": [],
      "end": "195.120",
      "id": "f000038",
      "language": "hi",
      "lines": [
        "शशिनेवामलं रात्रौ गगनं तोयदात्यये"
      ]
    },
    {
      "begin": "195.120",
      "children": [],
      "end": "199.680",
      "id": "f000039",
      "language": "hi",
      "lines": [
        "तदिदं न विभात्यद्य विहीनं तेन धीमता"
      ]
    },
    {
      "begin": "199.680",
      "children": [],
      "end": "204.280",
      "id": "f000040",
      "language": "hi",
      "lines": [
        "व्योमेव शशिना हीनमप्च्छुष्क इव सागरः"
      ]
    },
    {
      "begin": "204.280",
      "children": [],
      "end": "209.440",
      "id": "f000041",
      "language": "hi",
      "lines": [
        "बाष्पमुत्सृज्य कण्ठेन स्वार्त्तः परमपीडितः"
      ]
    },
    {
      "begin": "209.440",
      "children": [],
      "end": "214.480",
      "id": "f000042",
      "language": "hi",
      "lines": [
        "प्रच्छाद्य वदनं श्रीमद्वस्त्रेण जयतां वरः"
      ]
    },
    {
      "begin": "214.480",
      "children": [],
      "end": "219.280",
      "id": "f000043",
      "language": "hi",
      "lines": [
        "तमार्त्तं देवसङ्काशं समीक्ष्य पतितं भुवि"
      ]
    },
    {
      "begin": "219.280",
      "children": [],
      "end": "223.680",
      "id": "f000044",
      "language": "hi",
      "lines": [
        "निकृत्तमिव सालस्य स्कन्धं परशुना वने"
      ]
    },
    {
      "begin": "223.680",
      "children": [],
      "end": "228.680",
      "id": "f000045",
      "language": "hi",
      "lines": [
        "मत्तमातङ्गसङ्काशं चन्द्रार्कसदृशं भुवः"
      ]
    },
    {
      "begin": "228.680",
      "children": [],
      "end": "233.800",
      "id": "f000046",
      "language": "hi",
      "lines": [
        "उत्थापयित्वा शोकार्तं वचनं चेदमब्रवीत्"
      ]
    },
    {
      "begin": "233.800",
      "children": [],
      "end": "239.040",
      "id": "f000047",
      "language": "hi",
      "lines": [
        "उत्तिष्ठोत्तिष्ठ किं शेषे राजन्नत्र महायशः"
      ]
    },
    {
      "begin": "239.040",
      "children": [],
      "end": "243.600",
      "id": "f000048",
      "language": "hi",
      "lines": [
        "त्वद्विधा नहि शोचन्ति सन्तः सदसि सम्मताः"
      ]
    },
    {
      "begin": "243.600",
      "children": [],
      "end": "247.960",
      "id": "f000049",
      "language": "hi",
      "lines": [
        "दानयज्ञाधिकारा हि शीलश्रुतिवचोनुगा"
      ]
    },
    {
      "begin": "247.960",
      "children": [],
      "end": "252.800",
      "id": "f000050",
      "language": "hi",
      "lines": [
        "बुद्धिस्ते बुद्धिसम्पन्न प्रभेवार्कस्य मन्दिरे"
      ]
    },
    {
      "begin": "252.800",
      "children": [],
      "end": "257.280",
      "id": "f000051",
      "language": "hi",
      "lines": [
        "स रुदित्वा चिरं कालं भूमौ विपरिवृत्य च"
      ]
    },
    {
      "begin": "257.280",
      "children": [],
      "end": "260.960",
      "id": "f000052",
      "language": "hi",
      "lines": [
        "जननीं प्रत्युवाचेदं शोकैर्बहुभिरावृतः"
      ]
    },
    {
      "begin": "260.960",
      "children": [],
      "end": "266.440",
      "id": "f000053",
      "language": "hi",
      "lines": [
        "अभिषेक्ष्यति राम नु राजा यज्ञं नु यक्ष्यते"
      ]
    },
    {
      "begin": "266.440",
      "children": [],
      "end": "271.160",
      "id": "f000054",
      "language": "hi",
      "lines": [
        "इत्यहं कृतसङ्कल्पो हृष्टो यात्रामयासिषम्"
      ]
    },
    {
      "begin": "271.160",
      "children": [],
      "end": "276.040",
      "id": "f000055",
      "language": "hi",
      "lines": [
        "तदिदं ह्यन्यथाभूतं व्यवदीर्णं मनो मम"
      ]
    },
    {
      "begin": "276.040",
      "children": [],
      "end": "279.800",
      "id": "f000056",
      "language": "hi",
      "lines": [
        "पितरं यो न पश्यामि नित्यं प्रियहितेरतम्"
      ]
    },
    {
      "begin": "279.800",
      "children": [],
      "end": "285.800",
      "id": "f000057",
      "language": "hi",
      "lines": [
        "अम्ब केनात्यगाद्राजा व्याधिना मय्यनागते"
      ]
    },
    {
      "begin": "285.800",
      "children": [],
      "end": "290.320",
      "id": "f000058",
      "language": "hi",
      "lines": [
        "धन्या रामादयः सर्वे यैः पिता संस्कृतः स्वयम्"
      ]
    },
    {
      "begin": "290.320",
      "children": [],
      "end": "296.480",
      "id": "f000059",
      "language": "hi",
      "lines": [
        "न नूनं मां महाराजः प्राप्तं जानाति कीर्तिमान्"
      ]
    },
    {
      "begin": "296.480",
      "children": [],
      "end": "301.160",
      "id": "f000060",
      "language": "hi",
      "lines": [
        "उपजिघ्रेद्धि मां मूर्ध्नि तातः सन्नम्य सत्वरम्"
      ]
    },
    {
      "begin": "301.160",
      "children": [],
      "end": "306.760",
      "id": "f000061",
      "language": "hi",
      "lines": [
        "क्व स पाणिः सुखस्पर्शस्तातस्याक्लिष्टकर्मणः"
      ]
    },
    {
      "begin": "306.760",
      "children": [],
      "end": "311.520",
      "id": "f000062",
      "language": "hi",
      "lines": [
        "येन मां रजसा ध्वस्तमभीक्ष्णं परिमार्जति"
      ]
    },
    {
      "begin": "311.520",
      "children": [],
      "end": "315.760",
      "id": "f000063",
      "language": "hi",
      "lines": [
        "यो मे भ्राता पिता बन्धुर्यस्य दासोऽस्मि धीमतः"
      ]
    },
    {
      "begin": "315.760",
      "children": [],
      "end": "322.760",
      "id": "f000064",
      "language": "hi",
      "lines": [
        "तस्य मां शीघ्रमाख्याहि रामस्याक्लिष्टकर्मणः"
      ]
    },
    {
      "begin": "322.760",
      "children": [],
      "end": "327.880",
      "id": "f000065",
      "language": "hi",
      "lines": [
        "पिता हि भवति ज्येष्ठो धर्ममार्यस्य जानतः"
      ]
    },
    {
      "begin": "327.880",
      "children": [],
      "end": "333.080",
      "id": "f000066",
      "language": "hi",
      "lines": [
        "तस्य पादौ ग्रहीष्यामि स हीदानीं गतिर्मम"
      ]
    },
    {
      "begin": "333.080",
      "children": [],
      "end": "337.920",
      "id": "f000067",
      "language": "hi",
      "lines": [
        "धर्मविद्धर्मनित्यश्च सत्यसन्धो दृढव्रतः"
      ]
    },
    {
      "begin": "337.920",
      "children": [],
      "end": "343.160",
      "id": "f000068",
      "language": "hi",
      "lines": [
        "आर्यः किमब्रवीद्राजा पिता मे सत्यविक्रमः"
      ]
    },
    {
      "begin": "343.160",
      "children": [],
      "end": "348.440",
      "id": "f000069",
      "language": "hi",
      "lines": [
        "पश्चिमं साधु सन्देशमिच्छामि श्रोतुमात्मनः"
      ]
    },
    {
      "begin": "348.440",
      "children": [],
      "end": "353.320",
      "id": "f000070",
      "language": "hi",
      "lines": [
        "इति पृष्टा यथातत्त्वं कैकेयी वाक्यमब्रवीत्"
      ]
    },
    {
      "begin": "353.320",
      "children": [],
      "end": "357.280",
      "id": "f000071",
      "language": "hi",
      "lines": [
        "रामेति राजा विलपन् हा सीते लक्ष्मणेति च"
      ]
    },
    {
      "begin": "357.280",
      "children": [],
      "end": "363.400",
      "id": "f000072",
      "language": "hi",
      "lines": [
        "स महात्मा परं लोकं गतो गतिमतां वरः"
      ]
    },
    {
      "begin": "363.400",
      "children": [],
      "end": "368.720",
      "id": "f000073",
      "language": "hi",
      "lines": [
        "इमां तु पश्चिमां वाचं व्याजहार पिता तव"
      ]
    },
    {
      "begin": "368.720",
      "children": [],
      "end": "372.840",
      "id": "f000074",
      "language": "hi",
      "lines": [
        "कालधर्मपरिक्षिप्तः पाशैरिव महागजः"
      ]
    },
    {
      "begin": "372.840",
      "children": [],
      "end": "377.880",
      "id": "f000075",
      "language": "hi",
      "lines": [
        "सिद्धार्थास्ते नरा राममागतं सीतया सह"
      ]
    },
    {
      "begin": "377.880",
      "children": [],
      "end": "381.880",
      "id": "f000076",
      "language": "hi",
      "lines": [
        "लक्ष्मणं च महाबाहुं द्रक्ष्यन्ति पुनरागतम्"
      ]
    },
    {
      "begin": "381.880",
      "children": [],
      "end": "387.320",
      "id": "f000077",
      "language": "hi",
      "lines": [
        "तच्छ्रुत्वा विषसादैव द्वितीयाप्रियशंसनात्"
      ]
    },
    {
      "begin": "387.320",
      "children": [],
      "end": "391.480",
      "id": "f000078",
      "language": "hi",
      "lines": [
        "विषण्णवदनो भूत्वा भूयः पप्रच्छ मातरम्"
      ]
    },
    {
      "begin": "391.480",
      "children": [],
      "end": "397.320",
      "id": "f000079",
      "language": "hi",
      "lines": [
        "क्व चेदानीं स धर्मात्मा कौसल्यानन्दवर्द्धनः"
      ]
    },
    {
      "begin": "397.320",
      "children": [],
      "end": "402.080",
      "id": "f000080",
      "language": "hi",
      "lines": [
        "लक्ष्मणेन सह भ्रात्रा सीतया च समं गतः"
      ]
    },
    {
      "begin": "402.080",
      "children": [],
      "end": "406.960",
      "id": "f000081",
      "language": "hi",
      "lines": [
        "तथा पृष्टा यथातत्त्वमाख्यातुमुपचक्रमे"
      ]
    },
    {
      "begin": "406.960",
      "children": [],
      "end": "411.680",
      "id": "f000082",
      "language": "hi",
      "lines": [
        "मातास्य युगपद्वाक्यं विप्रियं प्रियशङ्कया"
      ]
    },
    {
      "begin": "411.680",
      "children": [],
      "end": "416.240",
      "id": "f000083",
      "language": "hi",
      "lines": [
        "स हि राजसुतः पुत्र चीरवासा महावनम्"
      ]
    },
    {
      "begin": "416.240",
      "children": [],
      "end": "420.880",
      "id": "f000084",
      "language": "hi",
      "lines": [
        "दण्डकान् सह वैदेह्या लक्ष्मणानुचरो गतः"
      ]
    },
    {
      "begin": "420.880",
      "children": [],
      "end": "426.000",
      "id": "f000085",
      "language": "hi",
      "lines": [
        "तच्छ्रुत्वा भरतस्रस्तो भ्रातुश्चारित्रशङ्कया"
      ]
    },
    {
      "begin": "426.000",
      "children": [],
      "end": "436.440",
      "id": "f000086",
      "language": "hi",
      "lines": [
        "स्वस्य वंशस्य माहात्म्यात् प्रष्टुं समुपचक्रमे"
      ]
    },
    {
      "begin": "436.440",
      "children": [],
      "end": "445.640",
      "id": "f000087",
      "language": "hi",
      "lines": [
        "कच्चिन्न परदारान्वा राजपुत्रोऽभिमन्यते"
      ]
    },
    {
      "begin": "445.640",
      "children": [],
      "end": "450.080",
      "id": "f000088",
      "language": "hi",
      "lines": [
        "कस्मात्स दण्डकारण्ये भ्रूणहेव विवासितः"
      ]
    },
    {
      "begin": "450.080",
      "children": [],
      "end": "455.160",
      "id": "f000089",
      "language": "hi",
      "lines": [
        "अथास्य चपला माता तत् स्वकर्म यथातथम्"
      ]
    },
    {
      "begin": "455.160",
      "children": [],
      "end": "460.480",
      "id": "f000090",
      "language": "hi",
      "lines": [
        "तेनैव स्त्रीस्वभावेन व्याहर्तुमुपचक्रमे"
      ]
    },
    {
      "begin": "460.480",
      "children": [],
      "end": "465.240",
      "id": "f000091",
      "language": "hi",
      "lines": [
        "एवमुक्ता तु कैकेयी भरतेन महात्मना"
      ]
    },
    {
      "begin": "465.240",
      "children": [],
      "end": "468.320",
      "id": "f000092",
      "language": "hi",
      "lines": [
        "उचाच वचनं हृटा मूढा पण्डितमानिनी"
      ]
    },
    {
      "begin": "468.320",
      "children": [],
      "end": "474.520",
      "id": "f000093",
      "language": "hi",
      "lines": [
        "कच्चिन्नाढ्यो दरिद्रो वा तेनापापो विहिंसितः"
      ]
    },
    {
      "begin": "474.520",
      "children": [],
      "end": "484.000",
      "id": "f000094",
      "language": "hi",
      "lines": [
        "न रामः परदारांश्च चक्षुर्भ्यामपि पश्यति"
      ]
    },
    {
      "begin": "484.000",
      "children": [],
      "end": "488.160",
      "id": "f000095",
      "language": "hi",
      "lines": [
        "मया तु पुत्र श्रुत्वैव रामस्यैवाभिषेचनम्"
      ]
    },
    {
      "begin": "488.160",
      "children": [],
      "end": "493.200",
      "id": "f000096",
      "language": "hi",
      "lines": [
        "याचितस्ते पिता राज्यं रामस्य च विवासनम्"
      ]
    },
    {
      "begin": "493.200",
      "children": [],
      "end": "498.240",
      "id": "f000097",
      "language": "hi",
      "lines": [
        "स स्ववृत्तिं समास्थाय पिता ते तत्तथाकरोत्"
      ]
    },
    {
      "begin": "498.240",
      "children": [],
      "end": "502.640",
      "id": "f000098",
      "language": "hi",
      "lines": [
        "रामश्च सहसौमित्रिः प्रेषितः सीतया सह"
      ]
    },
    {
      "begin": "502.640",
      "children": [],
      "end": "507.400",
      "id": "f000099",
      "language": "hi",
      "lines": [
        "तमपश्यन् प्रियं पुत्रं महीपालो महायशाः"
      ]
    },
    {
      "begin": "507.400",
      "children": [],
      "end": "511.920",
      "id": "f000100",
      "language": "hi",
      "lines": [
        "पुत्रशोकपरिद्यूनः पञ्चत्वमुपपेदिवान्"
      ]
    },
    {
      "begin": "511.920",
      "children": [],
      "end": "517.040",
      "id": "f000101",
      "language": "hi",
      "lines": [
        "त्वया त्विदानीं धर्मज्ञ राजत्वमवलम्ब्यताम्"
      ]
    },
    {
      "begin": "517.040",
      "children": [],
      "end": "521.000",
      "id": "f000102",
      "language": "hi",
      "lines": [
        "त्वत्कृते हि मया सर्वमिदमेवं विधं कृतम्"
      ]
    },
    {
      "begin": "521.000",
      "children": [],
      "end": "528.960",
      "id": "f000103",
      "language": "hi",
      "lines": [
        "मा शोकं मा च सन्तापं धैर्यमाश्रय पुत्रक"
      ]
    },
    {
      "begin": "528.960",
      "children": [],
      "end": "538.080",
      "id": "f000104",
      "language": "hi",
      "lines": [
        "त्वदधीना हि नगरी राज्यं चैतदनामयम्"
      ]
    },
    {
      "begin": "538.080",
      "children": [],
      "end": "549.160",
      "id": "f000105",
      "language": "hi",
      "lines": [
        "तत्पुत्र शीघ्रं विधिना विधिज्ञैर्वसिष्ठमुख्यैः सहितो द्विजेन्द्रैः"
      ]
    },
    {
      "begin": "549.160",
      "children": [],
      "end": "561.200",
      "id": "f000106",
      "language": "hi",
      "lines": [
        "सङ्काल्य राजानमदीनसत्त्वमात्मानमुर्व्यामभिषेचयस्व"
      ]
    }
  ]
}