    },
    {
      "begin": "430.000",
      "children": [],
      "end": "437.000",
      "id": "f000073",
      "language": "hi",
      "lines": [
        "क्वचित्प्रनृत्तैः क्वचिदुन्नदद्भिः क्वचिच्च वृक्षाग्रनिषण्णकायैः"
      ]
    },
    {
      "begin": "437.000",
      "children": [],
      "end": "443.280",
      "id": "f000074",
      "language": "hi",
      "lines": [
