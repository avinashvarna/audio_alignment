      "language": "hi",
      "lines": [
        "ततो गृध्रस्य वचनात्सम्पातेर्हनुमान् बली"
      ]
    },
    {
      "begin": "694.160",
      "children": [],
      "end": "698.440",
      "id": "f000144",
      "language": "hi",
      "lines": [
        "शतयोजनविस्तीर्णं पुप्लुवे लवणार्णवम्"
      ]
    },
    {
      "begin": "698.440",
      "children": [],
      "end": "703.960",
      "id": "f000145",
      "language": "hi",
      "lines": [
        "तत्र लङ्कां समासाद्य पुरीं रावणपालिताम्"
      ]
    },
    {
      "begin": "703.960",
      "children": [],
      "end": "708.920",
      "id": "f000146",
      "language": "hi",
      "lines": [
        "ददर्श सीतां ध्यायन्तीमशोकवनिकां गताम्"
      ]
    },
    {
      "begin": "708.920",
      "children": [],
      "end": "713.440",
      "id": "f000147",
      "language": "hi",
      "lines": [
        "निवेदयित्वाभिज्ञानं प्रवृत्तिं च निवेद्य च"
      ]
    },
    {
      "begin": "713.440",
      "children": [],
      "end": "717.840",
      "id": "f000148",
      "language": "hi",
      "lines": [
        "समाश्वास्य च वैदेहीं मर्दयामास तोरणम्"
      ]
    },
    {
      "begin": "717.840",
      "children": [],
      "end": "722.840",
      "id": "f000149",
      "language": "hi",
      "lines": [
        "पञ्च सेनाग्रगान् हत्वा सप्त मन्त्रिसुतानपि"
      ]
    },
    {
      "begin": "722.840",
      "children": [],
      "end": "727.000",
      "id": "f000150",
      "language": "hi",
      "lines": [
        "शूरमक्षं च निष्पिष्य ग्रहणं समुपागमत्"
      ]
    },
    {
      "begin": "727.000",
      "children": [],
      "end": "732.960",
      "id": "f000151",
      "language": "hi",
      "lines": [
        "अस्त्रेणोन्मुक्तमात्मानं ज्ञात्वा पैतामहाद्वरात्"
      ]
    },
    {
      "begin": "732.960",
      "children": [],
      "end": "737.920",
      "id": "f000152",
      "language": "hi",
      "lines": [
        "मर्षयन् राक्षसान् वीरो यन्त्रिणस्तान् यदृच्छया"
      ]
    },
    {
      "begin": "737.920",
      "children": [],
      "end": "742.760",
      "id": "f000153",
      "language": "hi",
      "lines": [
        "ततो दग्ध्वा पुरीं लङ्कामृते सीतां च मैथिलीम्"
      ]
    },
    {
      "begin": "742.760",
      "children": [],
      "end": "747.920",
      "id": "f000154",
      "language": "hi",
      "lines": [
        "रामाय प्रियमाख्यातुं पुनरायान्महाकपिः"
      ]
    },
    {
      "begin": "747.920",
      "children": [],
      "end": "752.040",
      "id": "f000155",
      "language": "hi",
      "lines": [
        "सोऽभिगम्य महात्मानं कृत्वा रामं प्रदक्षिणम्"
      ]
    },
    {
      "begin": "752.040",
      "children": [],
      "end": "757.480",
      "id": "f000156",
      "language": "hi",
      "lines": [
        "न्यवेदयदमेयात्मा दृष्टा सीतेति तत्त्वतः"
      ]
    },
    {
      "begin": "757.480",
      "children": [],
      "end": "762.080",
      "id": "f000157",
      "language": "hi",
      "lines": [
        "ततः सुग्रीवसहितो गत्वा तीरं महोदधेः"
      ]
    },
    {
      "begin": "762.080",
      "children": [],
      "end": "766.760",
      "id": "f000158",
      "language": "hi",
      "lines": [
        "समुद्रं क्षोभयामास शरैरादित्यसन्निभैः"
      ]
    },
    {
      "begin": "766.760",
      "children": [],
      "end": "771.480",
      "id": "f000159",
      "language": "hi",
      "lines": [
        "दर्शयामास चात्मानं समुद्रः सरितां पतिः"
      ]
    },
    {
      "begin": "771.480",
      "children": [],
      "end": "775.480",
      "id": "f000160",
      "language": "hi",
      "lines": [
        "समुद्रवचनाच्चैव नलं सेतुमकारयत्"
      ]
    },
    {
      "begin": "775.480",
      "children": [],
      "end": "780.280",
      "id": "f000161",
      "language": "hi",
      "lines": [
        "तेन गत्वा पुरीं लङ्कां हत्वा रावणमाहवे"
      ]
    },
    {
      "begin": "780.280",
      "children": [],
      "end": "785.160",
      "id": "f000162",
      "language": "hi",
      "lines": [
        "रामः सीतामनुप्राप्य परां व्रीडामुपागमत्"
      ]
    },
    {
      "begin": "785.160",
      "children": [],
      "end": "789.160",
      "id": "f000163",
      "language": "hi",
      "lines": [
        "तामुवाच ततो रामः परुषं जनसंसदि"
      ]
    },
    {
      "begin": "789.160",
      "children": [],
      "end": "793.640",
      "id": "f000164",
      "language": "hi",
      "lines": [
        "अमृष्यमाणा सा सीता विवेश ज्वलनं सती"
      ]
    },
    {
      "begin": "793.640",
      "children": [],
      "end": "798.600",
      "id": "f000165",
      "language": "hi",
      "lines": [
        "ततोऽग्निवचनात्सीतां ज्ञात्वा विगतकल्मषाम्"
      ]
    },
    {
      "begin": "798.600",
      "children": [],
      "end": "803.360",
      "id": "f000166",
      "language": "hi",
      "lines": [
        "कर्मणा तेन महता त्रैलोक्यं सचराचरम्"
      ]
    },
    {
      "begin": "803.360",
      "children": [],
      "end": "810.440",
      "id": "f000167",
      "language": "hi",
      "lines": [
        "सदेवर्षिगणं तुष्टं राघवस्य महात्मनः"
      ]
    },
    {
      "begin": "810.440",
      "children": [],
      "end": "817.000",
      "id": "f000168",
      "language": "hi",
      "lines": [
        "अभिषिच्य च लङ्कायां राक्षसेन्द्रं विभीषणम्"
      ]
    },
    {
      "begin": "817.000",
      "children": [],
      "end": "821.880",
      "id": "f000169",
      "language": "hi",
      "lines": [
        "कृतकृत्यस्तदा रामो विज्वरः प्रमुमोद ह"
      ]
    },
    {
      "begin": "821.880",
      "children": [],
      "end": "826.120",
      "id": "f000170",
      "language": "hi",
      "lines": [
        "देवताभ्यो वरं प्राप्य समुत्थाप्य च वानरान्"
      ]
    },
    {
      "begin": "826.120",
      "children": [],
      "end": "830.320",
      "id": "f000171",
      "language": "hi",
      "lines": [
        "अयोध्यां प्रस्थितो रामः पुष्पकेण सुहृद्वृतः"
      ]
    },
    {
      "begin": "830.320",
      "children": [],
      "end": "836.760",
      "id": "f000172",
      "language": "hi",
      "lines": [
        "भरद्वाजाश्रमं गत्वा रामः सत्यपराक्रमः"
      ]
    },
    {
      "begin": "836.760",
      "children": [],
      "end": "840.920",
      "id": "f000173",
      "language": "hi",
      "lines": [
        "भरतस्यान्तिकं रामो हनूमन्तं व्यसर्जयत्"
      ]
    },
    {
      "begin": "840.920",
      "children": [],
      "end": "845.600",
      "id": "f000174",
      "language": "hi",
      "lines": [
        "पुनराख्यायिकां जल्पन् सुग्रीवसहितस्तदा"
      ]
    },
    {
      "begin": "845.600",
      "children": [],
      "end": "850.480",
      "id": "f000175",
      "language": "hi",
      "lines": [
        "पुष्पकं तत्समारुह्य नन्दिग्रामं ययौ तदा"
      ]
    },
    {
      "begin": "850.480",
      "children": [],
      "end": "855.200",
      "id": "f000176",
      "language": "hi",
      "lines": [
        "नन्दिग्रामे जटां हित्वा भ्रातृभिस्सहितोऽनघः"
      ]
    },
    {
      "begin": "855.200",
      "children": [],
      "end": "860.440",
      "id": "f000177",
      "language": "hi",
      "lines": [
        "रामः सीतामनुप्राप्य राज्यं पुनरवाप्तवान्"
      ]
    },
    {
      "begin": "860.440",
      "children": [],
      "end": "864.960",
      "id": "f000178",
      "language": "hi",
      "lines": [
        "प्रहृष्टमुदितो लोकस्तुष्टः पुष्टः सुधार्मिकः"
      ]
    },
    {
      "begin": "864.960",
      "children": [],
      "end": "869.360",
      "id": "f000179",
      "language": "hi",
      "lines": [
        "निरामयो ह्यरोगश्च दुर्भिक्षभयवर्जितः"
      ]
    },
    {
      "begin": "869.360",
      "children": [],
      "end": "872.600",
      "id": "f000180",
      "language": "hi",
      "lines": [
        "न पुत्रमरणं किञ्चिद् द्रक्ष्यन्ति पुरुषाः क्वचित्"
      ]
    },
    {
      "begin": "872.600",
      "children": [],
      "end": "878.200",
      "id": "f000181",
      "language": "hi",
      "lines": [
        "नार्यश्चाविधवा नित्यं भविष्यन्ति पतिव्रताः"
      ]
    },
    {
      "begin": "878.200",
      "children": [],
      "end": "883.080",
      "id": "f000182",
      "language": "hi",
      "lines": [
        "न चाग्निजं भयं किञ्चिन्नाप्सु मज्जन्ति जन्तवः"
      ]
    },
    {
      "begin": "883.080",
      "children": [],
      "end": "887.560",
      "id": "f000183",
      "language": "hi",
      "lines": [
        "न वातजं भयं किञ्चिन्नापि ज्वरकृतं तथा"
      ]
    },
    {
      "begin": "887.560",
      "children": [],
      "end": "891.760",
      "id": "f000184",
      "language": "hi",
      "lines": [
        "न चापि क्षुद्भयं तत्र न तस्करभयं तथा"
      ]
    },
    {
      "begin": "891.760",
      "children": [],
      "end": "895.480",
      "id": "f000185",
      "language": "hi",
      "lines": [
        "नगराणि च राष्ट्राणि धनधान्ययुतानि च"
      ]
    },
    {
      "begin": "895.480",
      "children": [],
      "end": "900.600",
      "id": "f000186",
      "language": "hi",
      "lines": [
        "नित्यं प्रमुदिताः सर्वे यथा कृतयुगे तथा"
      ]
    },
    {
      "begin": "900.600",
      "children": [],
      "end": "905.040",
      "id": "f000187",
      "language": "hi",
      "lines": [
        "अश्वमेधशतैरिष्ट्वा तथा बहुसुवर्णकैः"
      ]
    },
    {
      "begin": "905.040",
      "children": [],
      "end": "911.760",
      "id": "f000188",
      "language": "hi",
      "lines": [
        "गवां कोट्ययुतं दत्त्वा विद्वद्भ्यो विधिपूर्वकम्"
      ]
    },
    {
      "begin": "911.760",
      "children": [],
      "end": "920.520",
      "id": "f000189",
      "language": "hi",
      "lines": [
        "राजवंशाञ्छतगुणान् स्थापयिष्यति राघवः"
      ]
    },
    {
      "begin": "920.520",
      "children": [],
      "end": "926.120",
      "id": "f000190",
      "language": "hi",
      "lines": [
        "चातुर्वर्ण्यं च लोकेऽस्मिन् स्वेस्वे धर्मे नियोक्ष्यति"
      ]
    },
    {
      "begin": "926.120",
      "children": [],
      "end": "929.320",
      "id": "f000191",
      "language": "hi",
      "lines": [
        "दशवर्षसहस्राणि दशवर्षशतानि च"
      ]
    },
    {
      "begin": "929.320",
      "children": [],
      "end": "935.200",
      "id": "f000192",
      "language": "hi",
      "lines": [
        "रामो राज्यमुपासित्वा ब्रह्मलोकं गमिष्यति"
      ]
    },
    {
      "begin": "935.200",
      "children": [],
      "end": "939.600",
      "id": "f000193",
      "language": "hi",
      "lines": [
        "इदं पवित्रं पापघ्नं पुण्यं वेदैश्च सम्मितम्"
      ]
    },
    {
      "begin": "939.600",
      "children": [],
      "end": "945.280",
      "id": "f000194",
      "language": "hi",
      "lines": [
        "यः पठेद्रामचरितं सर्वपापैः प्रमुच्यते"
      ]
    },
    {
      "begin": "945.280",
      "children": [],
      "end": "950.160",
      "id": "f000195",
      "language": "hi",
      "lines": [
        "एतदाख्यानमायुष्यं पठन् रामायणं नरः"
      ]
    },
    {
      "begin": "950.160",
      "children": [],
      "end": "955.040",
      "id": "f000196",
      "language": "hi",
      "lines": [
        "सपुत्रपौत्रः सगणः प्रेत्य स्वर्गे महीयते"
      ]
    },
    {
      "begin": "955.040",
      "children": [],
      "end": "964.880",
      "id": "f000197",
      "language": "hi",
      "lines": [
        "पठन् द्विजो वागृषभत्वमीयात्स्यात् क्षत्रियो भूमिपतित्वमीयात्"
      ]
    },
    {
      "begin": "964.880",
      "children": [],
      "end": "982.560",
      "id": "f000198",
      "language": "hi",
      "lines": [
        "वणिग्जनः पण्यफलत्वमीयाज्जनश्च शूद्रोपि महत्त्वमीयात्"
      ]
    }
  ]
}