{
  "fragments": [
    {
      "begin": "0.000",
      "children": [],
      "end": "8.400",
      "id": "f000001",
      "language": "hi",
      "lines": [
        "तस्याः किञ्चित् करधृतमिव प्राप्त्वानीरशाखं"
      ]
    },
    {
      "begin": "8.400",
      "children": [],
      "end": "15.840",
      "id": "f000002",
      "language": "hi",
      "lines": [
        "नीत्वा नीलं सलिलवसनं मुक्तरोधोनितम्बम्"
      ]
    },
    {
      "begin": "15.840",
      "children": [],
      "end": "22.800",
      "id": "f000003",
      "language": "hi",
      "lines": [
        "प्रस्थानं ते कथमपि सखे लम्बमानस्य भावि"
      ]
    },
    {
      "begin": "22.800",
      "children": [],
      "end": "30.640",
      "id": "f000004",
      "language": "hi",
      "lines": [
        "ज्ञातास्वादो विवृतजघनां को विहातुं समर्थः"
      ]
    },
    {
      "begin": "30.640",
      "children": [],
      "end": "38.320",
      "id": "f000005",
      "language": "hi",
      "lines": [
        "त्वन्निष्यन्दोच्छ्वसितवसुधागन्धसम्पर्करम्यः"
      ]
    },
    {
      "begin": "38.320",
      "children": [],
      "end": "46.360",
      "id": "f000006",
      "language": "hi",
      "lines": [
        "स्रोतोरन्ध्रध्वनितसुभगं दन्तिभिः पीयमानः"
      ]
    },
    {
      "begin": "46.360",
      "children": [],
      "end": "54.800",
      "id": "f000007",
      "language": "hi",
      "lines": [
        "नीचैर्वास्यत्युपजिगमिषोः देवपूर्वं गिरिं ते"
      ]
    },
    {
      "begin": "54.800",
      "children": [],
      "end": "62.880",
      "id": "f000008",
      "language": "hi",
      "lines": [
        "शीतो वायुः परिणमयिता काननोदुम्बराणाम्"
      ]
    },
    {
      "begin": "62.880",
      "children": [],
      "end": "71.600",
      "id": "f000009",
      "language": "hi",
      "lines": [
        "तत्र स्कन्दं नियतवसतिं पुष्पमेघीकृतात्मा"
      ]
    },
    {
      "begin": "71.600",
      "children": [],
      "end": "80.000",
      "id": "f000010",
      "language": "hi",
      "lines": [
        "पुष्पासारैः स्नपयतु भवान् व्योमगङ्गाजलार्द्रैः"
      ]
    },
    {
      "begin": "80.000",
      "children": [],
      "end": "88.080",
      "id": "f000011",
      "language": "hi",
      "lines": [
        "रक्षाहेतोर्नवशशिभृता वासवीनां चमूनाम्"
      ]
    },
    {
      "begin": "88.080",
      "children": [],
      "end": "96.680",
      "id": "f000012",
      "language": "hi",
      "lines": [
        "अत्यादित्यं हुतवहमुखे संभृतं तद्धितेजः"
      ]
    },
    {
      "begin": "96.680",
      "children": [],
      "end": "104.880",
      "id": "f000013",
      "language": "hi",
      "lines": [
        "ज्योतिर्लेखावलयि गलितं यस्य बर्हं भवानी"
      ]
    },
    {
      "begin": "104.880",
      "children": [],
      "end": "111.960",
      "id": "f000014",
      "language": "hi",
      "lines": [
        "पुत्रप्रेम्णा कुवलयदलप्रापि कर्णे करोति"
      ]
    },
    {
      "begin": "111.960",
      "children": [],
      "end": "120.440",
      "id": "f000015",
      "language": "hi",
      "lines": [
        "धौतापाङ्गं हरशशिरुचा पावकेस्तं मयूरं"
      ]
    },
    {
      "begin": "120.440",
      "children": [],
      "end": "129.640",
      "id": "f000016",
      "language": "hi",
      "lines": [
        "पश्चादद्रिग्रहणगुरुभिः गर्जितैर्नर्तयेथाः"
      ]
    },
    {
      "begin": "129.640",
      "children": [],
      "end": "137.760",
      "id": "f000017",
      "language": "hi",
      "lines": [
        "आराध्यैनं शरवणभवं देवमुल्लङ्घिताध्वा"
      ]
    },
    {
      "begin": "137.760",
      "children": [],
      "end": "146.360",
      "id": "f000018",
      "language": "hi",
      "lines": [
        "सिद्धद्वन्द्वैर्जलकणभयाद् वीणिभिर्मुक्तमार्गः"
      ]
    },
    {
      "begin": "146.360",
      "children": [],
      "end": "153.600",
      "id": "f000019",
      "language": "hi",
      "lines": [
        "व्यालम्बेथाः सुरभितनयालम्भजां मानयिष्यन्"
      ]
    },
    {
      "begin": "153.600",
      "children": [],
      "end": "163.320",
      "id": "f000020",
      "language": "hi",
      "lines": [
        "स्रोतोमूर्त्या भुवि परिणतां रन्तिदेवस्य कीर्तिम्"
      ]
    },
    {
      "begin": "163.320",
      "children": [],
      "end": "171.280",
      "id": "f000021",
      "language": "hi",
      "lines": [
        "त्वय्यादातुं जलमवनते शार्ङ्गिणो वर्णचौरे"
      ]
    },
    {
      "begin": "171.280",
      "children": [],
      "end": "179.960",
      "id": "f000022",
      "language": "hi",
      "lines": [
        "तस्याः सिन्धोः पृथुमपि तनुं दूरभावात् प्रवाहम्"
      ]
    },
    {
      "begin": "179.960",
      "children": [],
      "end": "189.040",
      "id": "f000023",
      "language": "hi",
      "lines": [
        "प्रेक्षिष्यन्ते गगनगतयः नूनमावर्ज्य दृष्टीः"
      ]
    },
    {
      "begin": "189.040",
      "children": [],
      "end": "198.680",
      "id": "f000024",
      "language": "hi",
      "lines": [
        "एकं मुक्तागुणमिव भुवः स्थूलमध्येन्द्रनीलम्"
      ]
    },
    {
      "begin": "198.680",
      "children": [],
      "end": "206.680",
      "id": "f000025",
      "language": "hi",
      "lines": [
        "तामुत्तीर्य व्रज परिचितभ्रूलताविभ्रमाणां"
      ]
    },
    {
      "begin": "206.680",
      "children": [],
      "end": "214.120",
      "id": "f000026",
      "language": "hi",
      "lines": [
        "पक्ष्मोत्क्षेपाद् उपरिविलसत्कृष्णशारप्रभाणाम्"
      ]
    },
    {
      "begin": "214.120",
      "children": [],
      "end": "221.480",
      "id": "f000027",
      "language": "hi",
      "lines": [
        "कुन्दक्षेपानुगमधुकरश्रीमुषामात्मबिम्बं"
      ]
    },
    {
      "begin": "221.480",
      "children": [],
      "end": "230.560",
      "id": "f000028",
      "language": "hi",
      "lines": [
        "पात्रीकुर्वन् दशपुरवधूनेत्रकौतूहलानाम्"
      ]
    },
    {
      "begin": "230.560",
      "children": [],
      "end": "238.520",
      "id": "f000029",
      "language": "hi",
      "lines": [
        "ब्रह्मावर्तं जनपदम् अथ च्छायया गाहमानः"
      ]
    },
    {
      "begin": "238.520",
      "children": [],
      "end": "247.120",
      "id": "f000030",
      "language": "hi",
      "lines": [
        "क्षेत्रं क्षत्रप्रधनपिशुनं कौरवं तद्भजेथाः"
      ]
    },
    {
      "begin": "247.120",
      "children": [],
      "end": "255.480",
      "id": "f000031",
      "language": "hi",
      "lines": [
        "राजन्यानां शितशरशतैः यत्र गाण्डीवधन्वा"
      ]
    },
    {
      "begin": "255.480",
      "children": [],
      "end": "264.520",
      "id": "f000032",
      "language": "hi",
      "lines": [
        "धारापातैस्त्वमिव कमलान्यभ्यवर्षन् मुखानि"
      ]
    },
    {
      "begin": "264.520",
      "children": [],
      "end": "272.640",
      "id": "f000033",
      "language": "hi",
      "lines": [
        "हित्वा हालामभिमतरसां रेवतीलोचनाङ्कां"
      ]
    },
    {
      "begin": "272.640",
      "children": [],
      "end": "280.240",
      "id": "f000034",
      "language": "hi",
      "lines": [
        "बन्धुप्रीत्या समरविमुखः लाङ्गली याः सिषेवे"
      ]
    },
    {
      "begin": "280.240",
      "children": [],
      "end": "288.320",
      "id": "f000035",
      "language": "hi",
      "lines": [
        "कृत्वा तासामधिगममपां सौम्य सारस्वतीनाम्"
      ]
    },
    {
      "begin": "288.320",
      "children": [],
      "end": "295.560",
      "id": "f000036",
      "language": "hi",
      "lines": [
        "अन्तः शुद्धस्त्वमपि भविता वर्णमात्रेण कृष्णः"
      ]
    },
    {
      "begin": "295.560",
      "children": [],
      "end": "305.520",
      "id": "f000037",
      "language": "hi",
      "lines": [
        "तस्माद्गच्छेः अनुकनखलं शैलराजावतीर्णां"
      ]
    },
    {
      "begin": "305.520",
      "children": [],
      "end": "313.840",
      "id": "f000038",
      "language": "hi",
      "lines": [
        "जह्नोः कन्यां सगरतनयस्वर्गसोपानपङ्क्तिम्"
      ]
    },
    {
      "begin": "313.840",
      "children": [],
      "end": "321.680",
      "id": "f000039",
      "language": "hi",
      "lines": [
        "गौरीवक्त्रभ्रुकुटिरचनां या विहस्येव फेनैः"
      ]
    },
    {
      "begin": "321.680",
      "children": [],
      "end": "330.240",
      "id": "f000040",
      "language": "hi",
      "lines": [
        "शम्भोः केशग्रहणमकरोद् इन्दुलग्नोर्मिहस्ता"
      ]
    }
  ]
}