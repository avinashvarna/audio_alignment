{
  "fragments": [
    {
      "begin": "0.000",
      "children": [],
      "end": "8.320",
      "id": "f000001",
      "language": "hi",
      "lines": [
        "तस्य तद्वचनं श्रुत्वा विश्वामित्रस्य धीमतः"
      ]
    },
    {
      "begin": "8.320",
      "children": [],
      "end": "16.640",
      "id": "f000002",
      "language": "hi",
      "lines": [
        "हृष्टरोमा महातेजाः शतानन्दो महातपाः"
      ]
    },
    {
      "begin": "16.640",
      "children": [],
      "end": "21.320",
      "id": "f000003",
      "language": "hi",
      "lines": [
        "गौतमस्य सुतो ज्येष्ठस्तपसा द्योतितप्रभः"
      ]
    },
    {
      "begin": "21.320",
      "children": [],
      "end": "25.800",
      "id": "f000004",
      "language": "hi",
      "lines": [
        "रामसन्दर्शनादेव परं विस्मयमागतः"
      ]
    },
    {
      "begin": "25.800",
      "children": [],
      "end": "30.400",
      "id": "f000005",
      "language": "hi",
      "lines": [
        "स तौ निषण्णौ सम्प्रेक्ष्य सुखासीनौ नृपात्मजौ"
      ]
    },
    {
      "begin": "30.400",
      "children": [],
      "end": "35.360",
      "id": "f000006",
      "language": "hi",
      "lines": [
        "शतानन्दो मुनिश्रेष्ठं विश्वामित्रमथाब्रवीत्"
      ]
    },
    {
      "begin": "35.360",
      "children": [],
      "end": "39.440",
      "id": "f000007",
      "language": "hi",
      "lines": [
        "अपि ते मुनिशार्दूल मम माता यशस्विनी"
      ]
    },
    {
      "begin": "39.440",
      "children": [],
      "end": "42.760",
      "id": "f000008",
      "language": "hi",
      "lines": [
        "दर्शिता राजपुत्राय तपोदीर्घमुपागता"
      ]
    },
    {
      "begin": "42.760",
      "children": [],
      "end": "48.040",
      "id": "f000009",
      "language": "hi",
      "lines": [
        "अपि रामे महातेजा मम माता यशस्विनी"
      ]
    },
    {
      "begin": "48.040",
      "children": [],
      "end": "52.760",
      "id": "f000010",
      "language": "hi",
      "lines": [
        "वन्यैरुपाहरत्पूजां पूजार्हे सर्वदेहिनाम्"
      ]
    },
    {
      "begin": "52.760",
      "children": [],
      "end": "56.880",
      "id": "f000011",
      "language": "hi",
      "lines": [
        "अपि रामाय कथितं यथावृत्तं पुरातनम्"
      ]
    },
    {
      "begin": "56.880",
      "children": [],
      "end": "60.920",
      "id": "f000012",
      "language": "hi",
      "lines": [
        "मम मातुर्महातेजो दैवेन दुरनुष्ठितम्"
      ]
    },
    {
      "begin": "60.920",
      "children": [],
      "end": "65.400",
      "id": "f000013",
      "language": "hi",
      "lines": [
        "अपि कौशिक भद्रं ते गुरुणा मम सङ्गता"
      ]
    },
    {
      "begin": "65.400",
      "children": [],
      "end": "68.720",
      "id": "f000014",
      "language": "hi",
      "lines": [
        "माता मम मुनिश्रेष्ठ रामसन्दर्शनादितः"
      ]
    },
    {
      "begin": "68.720",
      "children": [],
      "end": "73.920",
      "id": "f000015",
      "language": "hi",
      "lines": [
        "अपि मे गुरुणा रामः पूजितः कुशिकात्मज"
      ]
    },
    {
      "begin": "73.920",
      "children": [],
      "end": "78.920",
      "id": "f000016",
      "language": "hi",
      "lines": [
        "इहागतो महातेजाः पूजां प्राप्तो महात्मनः"
      ]
    },
    {
      "begin": "78.920",
      "children": [],
      "end": "83.480",
      "id": "f000017",
      "language": "hi",
      "lines": [
        "अपि शान्तेन मनसा गुरुर्मे कुशिकात्मज"
      ]
    },
    {
      "begin": "83.480",
      "children": [],
      "end": "87.320",
      "id": "f000018",
      "language": "hi",
      "lines": [
        "इहागतेन रामेण प्रयतेनाभिवादितः"
      ]
    },
    {
      "begin": "87.320",
      "children": [],
      "end": "91.600",
      "id": "f000019",
      "language": "hi",
      "lines": [
        "तच्छ्रुत्वा वचनं तस्य विश्वामित्रो महामुनिः"
      ]
    },
    {
      "begin": "91.600",
      "children": [],
      "end": "95.800",
      "id": "f000020",
      "language": "hi",
      "lines": [
        "प्रत्युवाच शतानन्दं वाक्यज्ञो वाक्यकोविदम्"
      ]
    },
    {
      "begin": "95.800",
      "children": [],
      "end": "101.000",
      "id": "f000021",
      "language": "hi",
      "lines": [
        "नातिक्रान्तं मुनिश्रेष्ठ यत्कर्तव्यं कृतं मया"
      ]
    },
    {
      "begin": "101.000",
      "children": [],
      "end": "105.440",
      "id": "f000022",
      "language": "hi",
      "lines": [
        "सङ्गता मुनिना पत्नी भार्गवेणेव रेणुका"
      ]
    },
    {
      "begin": "105.440",
      "children": [],
      "end": "109.720",
      "id": "f000023",
      "language": "hi",
      "lines": [
        "तच्छ्रुत्वा वचनं तस्य विश्वामित्रस्य धीमतः"
      ]
    },
    {
      "begin": "109.720",
      "children": [],
      "end": "114.120",
      "id": "f000024",
      "language": "hi",
      "lines": [
        "शतानन्दो महातेजा रामं वचनमब्रवीत्"
      ]
    },
    {
      "begin": "114.120",
      "children": [],
      "end": "119.080",
      "id": "f000025",
      "language": "hi",
      "lines": [
        "स्वागतं ते नरश्रेष्ठ दिष्ट्या प्राप्तोऽसि राघव"
      ]
    },
    {
      "begin": "119.080",
      "children": [],
      "end": "122.720",
      "id": "f000026",
      "language": "hi",
      "lines": [
        "विश्वामित्रं पुरस्कृत्य महर्षिमपराजितम्"
      ]
    },
    {
      "begin": "122.720",
      "children": [],
      "end": "127.440",
      "id": "f000027",
      "language": "hi",
      "lines": [
        "अचिन्त्यकर्मा तपसा ब्रह्मर्षिरतुलप्रभः"
      ]
    },
    {
      "begin": "127.440",
      "children": [],
      "end": "132.480",
      "id": "f000028",
      "language": "hi",
      "lines": [
        "विश्वामित्रो महातेजा वेत्स्येनं परमां गतिम्"
      ]
    },
    {
      "begin": "132.480",
      "children": [],
      "end": "137.200",
      "id": "f000029",
      "language": "hi",
      "lines": [
        "नास्ति धन्यतरो राम त्वत्तोऽन्यो भुवि कश्चन"
      ]
    },
    {
      "begin": "137.200",
      "children": [],
      "end": "141.680",
      "id": "f000030",
      "language": "hi",
      "lines": [
        "गोप्ता कुशिकपुत्रस्ते येन तप्तं महत्तपः"
      ]
    },
    {
      "begin": "141.680",
      "children": [],
      "end": "146.680",
      "id": "f000031",
      "language": "hi",
      "lines": [
        "श्रूयतामभिधास्यामि कौशिकस्य महात्मनः"
      ]
    },
    {
      "begin": "146.680",
      "children": [],
      "end": "150.440",
      "id": "f000032",
      "language": "hi",
      "lines": [
        "यथा बलं यथा वृत्तं तन्मे निगदतः शृणु"
      ]
    },
    {
      "begin": "150.440",
      "children": [],
      "end": "155.680",
      "id": "f000033",
      "language": "hi",
      "lines": [
        "राजाभूदेष धर्मात्मा दीर्घकालमरिन्दमः"
      ]
    },
    {
      "begin": "155.680",
      "children": [],
      "end": "159.840",
      "id": "f000034",
      "language": "hi",
      "lines": [
        "धर्मज्ञः कृतविद्यश्च प्रजानां च हिते रतः"
      ]
    },
    {
      "begin": "159.840",
      "children": [],
      "end": "164.240",
      "id": "f000035",
      "language": "hi",
      "lines": [
        "प्रजापतिसुतस्त्वासीत् कुशो नाम महीपतिः"
      ]
    },
    {
      "begin": "164.240",
      "children": [],
      "end": "168.680",
      "id": "f000036",
      "language": "hi",
      "lines": [
        "कुशस्य पुत्रो बलवान् कुशनाभः सुधार्मिकः"
      ]
    },
    {
      "begin": "168.680",
      "children": [],
      "end": "173.240",
      "id": "f000037",
      "language": "hi",
      "lines": [
        "कुशनाभसुतस्त्वासीद्गाधिरित्येव विश्रुतः"
      ]
    },
    {
      "begin": "173.240",
      "children": [],
      "end": "178.160",
      "id": "f000038",
      "language": "hi",
      "lines": [
        "गाधेः पुत्रो महातेजा विश्वामित्रो महामुनिः"
      ]
    },
    {
      "begin": "178.160",
      "children": [],
      "end": "182.920",
      "id": "f000039",
      "language": "hi",
      "lines": [
        "विश्वामित्रो महातेजाः पालयामास मेदिनीम्"
      ]
    },
    {
      "begin": "182.920",
      "children": [],
      "end": "187.240",
      "id": "f000040",
      "language": "hi",
      "lines": [
        "बहुवर्षसहस्राणि राजा राज्यमकारयत्"
      ]
    },
    {
      "begin": "187.240",
      "children": [],
      "end": "191.560",
      "id": "f000041",
      "language": "hi",
      "lines": [
        "कदाचित्तु महातेजा योजयित्वा वरूथिनीम्"
      ]
    },
    {
      "begin": "191.560",
      "children": [],
      "end": "195.680",
      "id": "f000042",
      "language": "hi",
      "lines": [
        "अक्षौहिणीपरिवृतः परिचक्राम मेदिनीम्"
      ]
    },
    {
      "begin": "195.680",
      "children": [],
      "end": "199.680",
      "id": "f000043",
      "language": "hi",
      "lines": [
        "नगराणि च राष्ट्राणि सरितश्च तथा गिरीन्"
      ]
    },
    {
      "begin": "199.680",
      "children": [],
      "end": "204.120",
      "id": "f000044",
      "language": "hi",
      "lines": [
        "आश्रमान् क्रमशो राम विचरन्नाजगाम ह"
      ]
    },
    {
      "begin": "204.120",
      "children": [],
      "end": "208.200",
      "id": "f000045",
      "language": "hi",
      "lines": [
        "वसिष्ठस्याश्रमपदं नानावृक्षसुमाकुलम्"
      ]
    },
    {
      "begin": "208.200",
      "children": [],
      "end": "212.240",
      "id": "f000046",
      "language": "hi",
      "lines": [
        "नानामृगगणाकीर्णं सिद्धचारणसेवितम्"
      ]
    },
    {
      "begin": "212.240",
      "children": [],
      "end": "216.920",
      "id": "f000047",
      "language": "hi",
      "lines": [
        "देवदानवगन्धर्वैः किन्नरैरुपशोभितम्"
      ]
    },
    {
      "begin": "216.920",
      "children": [],
      "end": "221.200",
      "id": "f000048",
      "language": "hi",
      "lines": [
        "प्रशान्तहरिणाकीर्णं द्विजसङ्घनिषेवितम्"
      ]
    },
    {
      "begin": "221.200",
      "children": [],
      "end": "225.160",
      "id": "f000049",
      "language": "hi",
      "lines": [
        "ब्रह्मर्षिगणसङ्कीर्णं देवर्षिगणसेवितम्"
      ]
    },
    {
      "begin": "225.160",
      "children": [],
      "end": "229.560",
      "id": "f000050",
      "language": "hi",
      "lines": [
        "तपश्चरणसंसिद्धैरग्निकल्पैर्महात्मभिः"
      ]
    },
    {
      "begin": "229.560",
      "children": [],
      "end": "234.200",
      "id": "f000051",
      "language": "hi",
      "lines": [
        "सततं सङ्कुलं श्रीमद्ब्रह्मकल्पैर् महात्मभिः"
      ]
    },
    {
      "begin": "234.200",
      "children": [],
      "end": "239.200",
      "id": "f000052",
      "language": "hi",
      "lines": [
        "अब्भक्षैर्वायुभक्षैश्च शीर्णपर्णाशनैस्तथा"
      ]
    },
    {
      "begin": "239.200",
      "children": [],
      "end": "245.080",
      "id": "f000053",
      "language": "hi",
      "lines": [
        "फलमूलाशनैर्दान्तैर्जितरोषैर्जितेन्द्रियैः"
      ]
    },
    {
      "begin": "245.080",
      "children": [],
      "end": "254.040",
      "id": "f000054",
      "language": "hi",
      "lines": [
        "ऋषिभिर्वालखिल्यैश्च जपहोमपरायणैः"
      ]
    },
    {
      "begin": "254.040",
      "children": [],
      "end": "261.720",
      "id": "f000055",
      "language": "hi",
      "lines": [
        "अन्यैर्वैखानसैश्चैव समन्तादुपशोभितम्"
      ]
    },
    {
      "begin": "261.720",
      "children": [],
      "end": "264.960",
      "id": "f000056",
      "language": "hi",
      "lines": [
        "वसिष्ठस्याश्रमपदं ब्रह्मलोकमिवापरम्"
      ]
    },
    {
      "begin": "264.960",
      "children": [],
      "end": "277.160",
      "id": "f000057",
      "language": "hi",
      "lines": [
        "ददर्श जयतां श्रेष्ठो विश्वामित्रो महाबलः"
      ]
    }
  ]
}