{
  "fragments": [
    {
      "begin": "0.000",
      "children": [],
      "end": "8.160",
      "id": "f000001",
      "language": "hi",
      "lines": [
        "ततः प्रत्युषसि प्राप्ते प्राप्तधर्मार्थनिश्चयः"
      ]
    },
    {
      "begin": "8.160",
      "children": [],
      "end": "17.840",
      "id": "f000002",
      "language": "hi",
      "lines": [
        "राक्षसाधिपतेर्वेश्म भीमकर्मा विभीषणः"
      ]
    },
    {
      "begin": "17.840",
      "children": [],
      "end": "22.760",
      "id": "f000003",
      "language": "hi",
      "lines": [
        "शैलाग्रचयसङ्काशं शैलशृङ्गमिवोन्नतम्"
      ]
    },
    {
      "begin": "22.760",
      "children": [],
      "end": "27.760",
      "id": "f000004",
      "language": "hi",
      "lines": [
        "सुविभक्तमहाकक्ष्यं महाजनपरिग्रहम्"
      ]
    },
    {
      "begin": "27.760",
      "children": [],
      "end": "32.120",
      "id": "f000005",
      "language": "hi",
      "lines": [
        "मतिमद्भिर्महामात्रैरनुरक्तैरधिष्ठितम्"
      ]
    },
    {
      "begin": "32.120",
      "children": [],
      "end": "37.320",
      "id": "f000006",
      "language": "hi",
      "lines": [
        "राक्षसैश्चाप्तपर्याप्तैः सर्वतः परिरक्षितम्"
      ]
    },
    {
      "begin": "37.320",
      "children": [],
      "end": "42.800",
      "id": "f000007",
      "language": "hi",
      "lines": [
        "मत्तमातङ्गनिश्वासैर्व्याकुलीकृतमारुतम्"
      ]
    },
    {
      "begin": "42.800",
      "children": [],
      "end": "47.160",
      "id": "f000008",
      "language": "hi",
      "lines": [
        "शङ्खघोषमहाघोषं तूर्यनादानुनादितम्"
      ]
    },
    {
      "begin": "47.160",
      "children": [],
      "end": "52.120",
      "id": "f000009",
      "language": "hi",
      "lines": [
        "प्रमदाजनसम्बाधं प्रजल्पितमहापथम्"
      ]
    },
    {
      "begin": "52.120",
      "children": [],
      "end": "57.280",
      "id": "f000010",
      "language": "hi",
      "lines": [
        "तप्तकाञ्चननिर्यूहं भूषणोत्तमभूषितम्"
      ]
    },
    {
      "begin": "57.280",
      "children": [],
      "end": "62.040",
      "id": "f000011",
      "language": "hi",
      "lines": [
        "गन्धर्वाणामिवावासमालयं मरुतामिव"
      ]
    },
    {
      "begin": "62.040",
      "children": [],
      "end": "66.680",
      "id": "f000012",
      "language": "hi",
      "lines": [
        "रत्नसञ्चयसम्बाधं भवनं भोगिनामिव"
      ]
    },
    {
      "begin": "66.680",
      "children": [],
      "end": "72.200",
      "id": "f000013",
      "language": "hi",
      "lines": [
        "तं महाभ्रमिवादित्यस्तेजोविस्तृतरश्मिवान्"
      ]
    },
    {
      "begin": "72.200",
      "children": [],
      "end": "76.960",
      "id": "f000014",
      "language": "hi",
      "lines": [
        "अग्रजस्यालयं वीरः प्रविवेश महाद्युतिः"
      ]
    },
    {
      "begin": "76.960",
      "children": [],
      "end": "82.440",
      "id": "f000015",
      "language": "hi",
      "lines": [
        "पुण्यान् पुण्याहघोषांश्च वेदविद्भिरुदाहृतान्"
      ]
    },
    {
      "begin": "82.440",
      "children": [],
      "end": "87.600",
      "id": "f000016",
      "language": "hi",
      "lines": [
        "शुश्राव सुमहातेजा भ्रातुर्विजयसंश्रितान्"
      ]
    },
    {
      "begin": "87.600",
      "children": [],
      "end": "93.320",
      "id": "f000017",
      "language": "hi",
      "lines": [
        "पूजितान् दधिपात्रैश्च सर्पिर्भिः सुमनोक्षतैः"
      ]
    },
    {
      "begin": "93.320",
      "children": [],
      "end": "98.000",
      "id": "f000018",
      "language": "hi",
      "lines": [
        "मन्त्रवैदविदो विप्रान् ददर्श सुमहाबलः"
      ]
    },
    {
      "begin": "98.000",
      "children": [],
      "end": "103.560",
      "id": "f000019",
      "language": "hi",
      "lines": [
        "स पूज्यमानो रक्षोभिर्दीप्यमानः स्वतेजसा"
      ]
    },
    {
      "begin": "103.560",
      "children": [],
      "end": "108.360",
      "id": "f000020",
      "language": "hi",
      "lines": [
        "आसनस्थं महावाहुर्ववन्दे धनदानुजम्"
      ]
    },
    {
      "begin": "108.360",
      "children": [],
      "end": "113.120",
      "id": "f000021",
      "language": "hi",
      "lines": [
        "स राजदृष्टिसम्पन्नमासनं हेमभूषितम्"
      ]
    },
    {
      "begin": "113.120",
      "children": [],
      "end": "117.920",
      "id": "f000022",
      "language": "hi",
      "lines": [
        "जगाम समुदाचारं प्रयुज्याचारकोविदः"
      ]
    },
    {
      "begin": "117.920",
      "children": [],
      "end": "122.800",
      "id": "f000023",
      "language": "hi",
      "lines": [
        "स रावणं महात्मानं विजने मन्त्रिसन्निधौ"
      ]
    },
    {
      "begin": "122.800",
      "children": [],
      "end": "126.800",
      "id": "f000024",
      "language": "hi",
      "lines": [
        "उवाच हितमत्यर्थं वचनं हेतुनिश्चितम्"
      ]
    },
    {
      "begin": "126.800",
      "children": [],
      "end": "133.480",
      "id": "f000025",
      "language": "hi",
      "lines": [
        "प्रसाद्य भ्रातरं ज्येष्ठं सान्त्वेनोपस्थितक्रमः"
      ]
    },
    {
      "begin": "133.480",
      "children": [],
      "end": "138.440",
      "id": "f000026",
      "language": "hi",
      "lines": [
        "देशकालार्थसंवादी दृष्टलोकपरावरः"
      ]
    },
    {
      "begin": "138.440",
      "children": [],
      "end": "143.600",
      "id": "f000027",
      "language": "hi",
      "lines": [
        "यदाप्रभृति वैदेही सम्प्राप्तेमां पुरीं तव"
      ]
    },
    {
      "begin": "143.600",
      "children": [],
      "end": "148.600",
      "id": "f000028",
      "language": "hi",
      "lines": [
        "तदाप्रभृति दृश्यन्ते निमित्तान्यशुभानि नः"
      ]
    },
    {
      "begin": "148.600",
      "children": [],
      "end": "153.560",
      "id": "f000029",
      "language": "hi",
      "lines": [
        "सस्फुलिङ्गः सधूमार्चिः सधूमकलुषोदयः"
      ]
    },
    {
      "begin": "153.560",
      "children": [],
      "end": "158.360",
      "id": "f000030",
      "language": "hi",
      "lines": [
        "मन्त्रसन्धुक्षितोऽप्यग्निर्न सम्यगभिवर्धते"
      ]
    },
    {
      "begin": "158.360",
      "children": [],
      "end": "163.440",
      "id": "f000031",
      "language": "hi",
      "lines": [
        "अग्निष्ठेष्वग्निशालासु तथा ब्रह्मस्थलीषु च"
      ]
    },
    {
      "begin": "163.440",
      "children": [],
      "end": "168.680",
      "id": "f000032",
      "language": "hi",
      "lines": [
        "सरीसृपाणि दृश्यन्ते हव्येषु च पिपीलिकाः"
      ]
    },
    {
      "begin": "168.680",
      "children": [],
      "end": "173.840",
      "id": "f000033",
      "language": "hi",
      "lines": [
        "गवां पयांसि स्कन्नानि विमदा वीरकुञ्जराः"
      ]
    },
    {
      "begin": "173.840",
      "children": [],
      "end": "177.840",
      "id": "f000034",
      "language": "hi",
      "lines": [
        "दीनमश्वाः प्रहेषन्ते न च ग्रासाभिनन्दिनः"
      ]
    },
    {
      "begin": "177.840",
      "children": [],
      "end": "184.560",
      "id": "f000035",
      "language": "hi",
      "lines": [
        "खरोष्ट्राश्वतरा राजन् भिन्नरोमाः स्रवन्ति नः"
      ]
    },
    {
      "begin": "184.560",
      "children": [],
      "end": "189.680",
      "id": "f000036",
      "language": "hi",
      "lines": [
        "न स्वभावेऽवतिष्ठन्ते विधानैरपि चिन्तिताः"
      ]
    },
    {
      "begin": "189.680",
      "children": [],
      "end": "195.120",
      "id": "f000037",
      "language": "hi",
      "lines": [
        "वायसाः सङ्घशः क्रूरा व्याहरन्ति समन्ततः"
      ]
    },
    {
      "begin": "195.120",
      "children": [],
      "end": "200.560",
      "id": "f000038",
      "language": "hi",
      "lines": [
        "समवेताश्च दृश्यन्ते विमानाग्रेषु सङ्घशः"
      ]
    },
    {
      "begin": "200.560",
      "children": [],
      "end": "205.360",
      "id": "f000039",
      "language": "hi",
      "lines": [
        "गृध्राश्च परिलीयन्ते पुरीमुपरिपिण्डिताः"
      ]
    },
    {
      "begin": "205.360",
      "children": [],
      "end": "210.760",
      "id": "f000040",
      "language": "hi",
      "lines": [
        "उपपन्नाश्च सन्ध्ये द्वे व्याहरन्त्यशिवं शिवाः"
      ]
    },
    {
      "begin": "210.760",
      "children": [],
      "end": "216.320",
      "id": "f000041",
      "language": "hi",
      "lines": [
        "क्रव्यादानां मृगाणां च पुरद्वारेषु सङ्घशः"
      ]
    },
    {
      "begin": "216.320",
      "children": [],
      "end": "221.840",
      "id": "f000042",
      "language": "hi",
      "lines": [
        "श्रूयन्ते विपुला घोषाः सविस्फूर्जथुनिस्वनाः"
      ]
    },
    {
      "begin": "221.840",
      "children": [],
      "end": "226.800",
      "id": "f000043",
      "language": "hi",
      "lines": [
        "तदेवं प्रस्तुते कार्ये प्रायश्चित्तमिदं क्षमम्"
      ]
    },
    {
      "begin": "226.800",
      "children": [],
      "end": "232.480",
      "id": "f000044",
      "language": "hi",
      "lines": [
        "रोचते यदि वैदेही राघवाय प्रदीयताम्"
      ]
    },
    {
      "begin": "232.480",
      "children": [],
      "end": "238.400",
      "id": "f000045",
      "language": "hi",
      "lines": [
        "इदं च यदि वा मोहाल्लोभाद्वा व्याहृतं मया"
      ]
    },
    {
      "begin": "238.400",
      "children": [],
      "end": "242.360",
      "id": "f000046",
      "language": "hi",
      "lines": [
        "तत्रापि च महाराज न दोषं कर्तुमर्हसि"
      ]
    },
    {
      "begin": "242.360",
      "children": [],
      "end": "247.600",
      "id": "f000047",
      "language": "hi",
      "lines": [
        "अयं च दोषः सर्वस्य जनस्यास्योपलक्ष्यते"
      ]
    },
    {
      "begin": "247.600",
      "children": [],
      "end": "252.800",
      "id": "f000048",
      "language": "hi",
      "lines": [
        "रक्षसां राक्षसीनां च पुरस्यान्तःपुरस्य च"
      ]
    },
    {
      "begin": "252.800",
      "children": [],
      "end": "257.280",
      "id": "f000049",
      "language": "hi",
      "lines": [
        "श्रावणे चास्य मन्त्रस्य निवृत्ताः सर्वमन्त्रिणः"
      ]
    },
    {
      "begin": "257.280",
      "children": [],
      "end": "263.360",
      "id": "f000050",
      "language": "hi",
      "lines": [
        "अवश्यं च मया वाच्यं यद्दृष्टमपि वा श्रुतम्"
      ]
    },
    {
      "begin": "263.360",
      "children": [],
      "end": "268.680",
      "id": "f000051",
      "language": "hi",
      "lines": [
        "सम्प्रधार्य यथान्यायं तद्भवान् कर्तुमर्हति"
      ]
    },
    {
      "begin": "268.680",
      "children": [],
      "end": "274.440",
      "id": "f000052",
      "language": "hi",
      "lines": [
        "इति स्म मन्त्रिणां मध्ये भ्राता भ्रातरमूचिवान्"
      ]
    },
    {
      "begin": "274.440",
      "children": [],
      "end": "279.880",
      "id": "f000053",
      "language": "hi",
      "lines": [
        "रावणं राक्षसश्रेष्ठं पथ्यमेतद्विभीषणः"
      ]
    },
    {
      "begin": "279.880",
      "children": [],
      "end": "286.720",
      "id": "f000054",
      "language": "hi",
      "lines": [
        "हितं महार्थं मृदु हेतुसंहितं व्यतीतकालायतिसम्प्रतिक्षमम्"
      ]
    },
    {
      "begin": "286.720",
      "children": [],
      "end": "293.640",
      "id": "f000055",
      "language": "hi",
      "lines": [
        "निशम्य तद्वाक्यमुपस्थितज्वरः प्रसङ्गवानुत्तरमेतदब्रवीत्"
      ]
    },
    {
      "begin": "293.640",
      "children": [],
      "end": "300.520",
      "id": "f000056",
      "language": "hi",
      "lines": [
        "भयं न पश्यामि कुतश्चिदप्यहं न राघवः प्राप्स्यति जातु मैथिलीम्"
      ]
    },
    {
      "begin": "300.520",
      "children": [],
      "end": "310.360",
      "id": "f000057",
      "language": "hi",
      "lines": [
        "सुरैः सहेन्द्रैरपि सङ्गतः कथं ममाग्रतः स्थास्यति लक्ष्मणाग्रजः"
      ]
    },
    {
      "begin": "310.360",
      "children": [],
      "end": "324.120",
      "id": "f000058",
      "language": "hi",
      "lines": [
        "इतीदमुक्त्वा सुरसैन्यनाशनो महाबलः संयति चण्डविक्रमः"
      ]
    },
    {
      "begin": "324.120",
      "children": [],
      "end": "338.720",
      "id": "f000059",
      "language": "hi",
      "lines": [
        "दशाननो भ्रातरमाप्तवादिनं विसर्जयामास तदा विभीषणम्"
      ]
    }
  ]
}