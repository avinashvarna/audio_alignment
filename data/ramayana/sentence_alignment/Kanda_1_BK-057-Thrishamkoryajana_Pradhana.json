{
  "fragments": [
    {
      "begin": "0.000",
      "children": [],
      "end": "10.560",
      "id": "f000001",
      "language": "hi",
      "lines": [
        "ततः सन्तप्तहृदयः स्मरन् निग्रहमात्मनः"
      ]
    },
    {
      "begin": "10.560",
      "children": [],
      "end": "15.000",
      "id": "f000002",
      "language": "hi",
      "lines": [
        "विनिःश्वस्य विनिःश्वस्य कृतवैरो महात्मना"
      ]
    },
    {
      "begin": "15.000",
      "children": [],
      "end": "19.280",
      "id": "f000003",
      "language": "hi",
      "lines": [
        "स दक्षिणां दिशं गत्वा महिष्या सह राघव"
      ]
    },
    {
      "begin": "19.280",
      "children": [],
      "end": "24.240",
      "id": "f000004",
      "language": "hi",
      "lines": [
        "तताप परमं घोरं विश्वामित्रो महत्तपः"
      ]
    },
    {
      "begin": "24.240",
      "children": [],
      "end": "27.560",
      "id": "f000005",
      "language": "hi",
      "lines": [
        "फलमूलाशनो दान्तैश् चचार महत्तपः"
      ]
    },
    {
      "begin": "27.560",
      "children": [],
      "end": "33.440",
      "id": "f000006",
      "language": "hi",
      "lines": [
        "अथास्य जज्ञिरे पुत्राः सत्यधर्मपरायणाः"
      ]
    },
    {
      "begin": "33.440",
      "children": [],
      "end": "38.280",
      "id": "f000007",
      "language": "hi",
      "lines": [
        "हविष्यन्दो मधुष्यन्दो दृढनेत्रो महारथः"
      ]
    },
    {
      "begin": "38.280",
      "children": [],
      "end": "41.400",
      "id": "f000008",
      "language": "hi",
      "lines": [
        "पूर्णे वर्षसहस्रे तु ब्रह्मा लोकपितामहः"
      ]
    },
    {
      "begin": "41.400",
      "children": [],
      "end": "47.600",
      "id": "f000009",
      "language": "hi",
      "lines": [
        "अब्रवीन्मधुरं वाक्यं विश्वामित्रं तपोधनम्"
      ]
    },
    {
      "begin": "47.600",
      "children": [],
      "end": "51.720",
      "id": "f000010",
      "language": "hi",
      "lines": [
        "जिता राजर्षिलोकास्ते तपसा कुशिकात्मज"
      ]
    },
    {
      "begin": "51.720",
      "children": [],
      "end": "57.480",
      "id": "f000011",
      "language": "hi",
      "lines": [
        "अनेन तपसा त्वां तु राजर्षिरिति विद्महे"
      ]
    },
    {
      "begin": "57.480",
      "children": [],
      "end": "61.600",
      "id": "f000012",
      "language": "hi",
      "lines": [
        "एवमुक्त्वा महातेजा जगाम सह दैवतैः"
      ]
    },
    {
      "begin": "61.600",
      "children": [],
      "end": "66.320",
      "id": "f000013",
      "language": "hi",
      "lines": [
        "त्रिविष्टपं ब्रह्मलोकं लोकानां परमेश्वरः"
      ]
    },
    {
      "begin": "66.320",
      "children": [],
      "end": "71.240",
      "id": "f000014",
      "language": "hi",
      "lines": [
        "विश्वामित्रोपि तच्छ्रुत्वा ह्रिया किञ्चिदवाङ्मुखः"
      ]
    },
    {
      "begin": "71.240",
      "children": [],
      "end": "74.760",
      "id": "f000015",
      "language": "hi",
      "lines": [
        "दुःखेन महताविष्टः समन्युरिदमब्रवीत्"
      ]
    },
    {
      "begin": "74.760",
      "children": [],
      "end": "79.160",
      "id": "f000016",
      "language": "hi",
      "lines": [
        "तपश्च सुमहत्तप्तं राजर्षिरिति मां विदुः"
      ]
    },
    {
      "begin": "79.160",
      "children": [],
      "end": "83.480",
      "id": "f000017",
      "language": "hi",
      "lines": [
        "देवाः सर्षिगणाः सर्वे नास्ति मन्ये तपःफलम्"
      ]
    },
    {
      "begin": "83.480",
      "children": [],
      "end": "88.240",
      "id": "f000018",
      "language": "hi",
      "lines": [
        "एवं निश्चित्य मनसा भूय एव महातपाः"
      ]
    },
    {
      "begin": "88.240",
      "children": [],
      "end": "93.040",
      "id": "f000019",
      "language": "hi",
      "lines": [
        "तपश्चकार काकुत्स्थ परमं परमात्मवान्"
      ]
    },
    {
      "begin": "93.040",
      "children": [],
      "end": "97.520",
      "id": "f000020",
      "language": "hi",
      "lines": [
        "एतस्मिन्नेव काले तु सत्यवादी जितेन्द्रियः"
      ]
    },
    {
      "begin": "97.520",
      "children": [],
      "end": "101.480",
      "id": "f000021",
      "language": "hi",
      "lines": [
        "त्रिशङ्कुरिति विख्यात इक्ष्वाकुकुलवर्द्धनः"
      ]
    },
    {
      "begin": "101.480",
      "children": [],
      "end": "105.600",
      "id": "f000022",
      "language": "hi",
      "lines": [
        "तस्य बुद्धिः समुत्पन्ना यजेयमिति राघव"
      ]
    },
    {
      "begin": "105.600",
      "children": [],
      "end": "109.680",
      "id": "f000023",
      "language": "hi",
      "lines": [
        "गच्छेयं सशरीरेण देवानां परमां गतिम्"
      ]
    },
    {
      "begin": "109.680",
      "children": [],
      "end": "113.720",
      "id": "f000024",
      "language": "hi",
      "lines": [
        "स वसिष्ठं समाहूय कथयामास चिन्तितम्"
      ]
    },
    {
      "begin": "113.720",
      "children": [],
      "end": "117.960",
      "id": "f000025",
      "language": "hi",
      "lines": [
        "अशक्यमिति चाप्युक्तो वसिष्ठेन महात्मना"
      ]
    },
    {
      "begin": "117.960",
      "children": [],
      "end": "122.800",
      "id": "f000026",
      "language": "hi",
      "lines": [
        "प्रत्याख्यातो वसिष्ठेन स ययौ दक्षिणां दिशम्"
      ]
    },
    {
      "begin": "122.800",
      "children": [],
      "end": "127.400",
      "id": "f000027",
      "language": "hi",
      "lines": [
        "ततस्तत्कर्मसिद्ध्यर्थं पुत्रांस्तस्य गतो नृपः"
      ]
    },
    {
      "begin": "127.400",
      "children": [],
      "end": "131.520",
      "id": "f000028",
      "language": "hi",
      "lines": [
        "वासिष्ठा दीर्घतपसस् तपो यत्र हि तेपिरे"
      ]
    },
    {
      "begin": "131.520",
      "children": [],
      "end": "135.320",
      "id": "f000029",
      "language": "hi",
      "lines": [
        "त्रिशङ्कुः सुमहातेजाः शतं परमभास्वरम्"
      ]
    },
    {
      "begin": "135.320",
      "children": [],
      "end": "140.200",
      "id": "f000030",
      "language": "hi",
      "lines": [
        "वसिष्ठपुत्रान् ददृशे तप्यमानान् यशस्विनः"
      ]
    },
    {
      "begin": "140.200",
      "children": [],
      "end": "144.880",
      "id": "f000031",
      "language": "hi",
      "lines": [
        "सोऽभिगम्य महात्मानः सर्वान् एव गुरोः सुतान्"
      ]
    },
    {
      "begin": "144.880",
      "children": [],
      "end": "149.720",
      "id": "f000032",
      "language": "hi",
      "lines": [
        "अभिवाद्यानुपूर्व्येण ह्रिया किञ्चिदवाङ्मुखः"
      ]
    },
    {
      "begin": "149.720",
      "children": [],
      "end": "154.080",
      "id": "f000033",
      "language": "hi",
      "lines": [
        "अब्रवीत्सुमहात्मानः सर्वानेव कृताञ्जलिः"
      ]
    },
    {
      "begin": "154.080",
      "children": [],
      "end": "158.320",
      "id": "f000034",
      "language": "hi",
      "lines": [
        "शरणं वः प्रपद्येऽहं शरण्यान् शरणागतः"
      ]
    },
    {
      "begin": "158.320",
      "children": [],
      "end": "163.240",
      "id": "f000035",
      "language": "hi",
      "lines": [
        "प्रत्याख्यातोऽस्मि भद्रं वो वसिष्ठेन महात्मना"
      ]
    },
    {
      "begin": "163.240",
      "children": [],
      "end": "167.440",
      "id": "f000036",
      "language": "hi",
      "lines": [
        "यष्टुकामो महायज्ञं तदनुज्ञातुमर्हथ"
      ]
    },
    {
      "begin": "167.440",
      "children": [],
      "end": "171.600",
      "id": "f000037",
      "language": "hi",
      "lines": [
        "गुरुपुत्रानहं सर्वान्नमस्कृत्य प्रसादये"
      ]
    },
    {
      "begin": "171.600",
      "children": [],
      "end": "176.600",
      "id": "f000038",
      "language": "hi",
      "lines": [
        "शिरसा प्रणतो याचे ब्राह्मणान् तपसि स्थितान्"
      ]
    },
    {
      "begin": "176.600",
      "children": [],
      "end": "181.200",
      "id": "f000039",
      "language": "hi",
      "lines": [
        "ते मां भवन्तः सिद्ध्यर्थं याजयन्तु समाहिताः"
      ]
    },
    {
      "begin": "181.200",
      "children": [],
      "end": "185.400",
      "id": "f000040",
      "language": "hi",
      "lines": [
        "सशरीरो यथाहं हि देवलोकमवाप्नुयाम्"
      ]
    },
    {
      "begin": "185.400",
      "children": [],
      "end": "190.560",
      "id": "f000041",
      "language": "hi",
      "lines": [
        "प्रत्याख्यातो वसिष्ठेन गतिमन्यां तपोधनाः"
      ]
    },
    {
      "begin": "190.560",
      "children": [],
      "end": "196.800",
      "id": "f000042",
      "language": "hi",
      "lines": [
        "गुरुपुत्रानृते सर्वान्नाहं पश्यामि काञ्चन"
      ]
    },
    {
      "begin": "196.800",
      "children": [],
      "end": "210.160",
      "id": "f000043",
      "language": "hi",
      "lines": [
        "इक्ष्वाकूणां हि सर्वेषां पुरोधाः परमा गतिः"
      ]
    },
    {
      "begin": "210.160",
      "children": [],
      "end": "219.040",
      "id": "f000044",
      "language": "hi",
      "lines": [
        "तस्मादनन्तरं सर्वे भवन्तो दैवतं मम"
      ]
    }
  ]
}