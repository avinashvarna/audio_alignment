    {
      "begin": "195.240",
      "children": [],
      "end": "214.480",
      "id": "f000023",
      "language": "hi",
      "lines": [
        "तमरण्यसमाश्रयोमन्मुखं शिरसा वेष्टनशोभिना सुतः । पितरं प्रणिपत्य पादयोरपरित्यागमयाचतात्मनः ॥"
      ]
    },
    {
      "begin": "214.480",
      "children": [],
      "end": "215.000",
      "id": "f000024",
      "language": "hi",
      "lines": [
