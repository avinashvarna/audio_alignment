    {
      "begin": "1714.800",
      "children": [],
      "end": "1737.400",
      "id": "f000183",
      "language": "hi",
      "lines": [
        "तेनाष्टौ परिगमिताः समाः कथंचिद्वालत्वादवितथसूनृतेन सूनोः । सादृश्यप्रतिकृतिदर्शनैः प्रियायाः स्वप्नेषु क्षणिकसमागमोत्सवैश्च ॥"
      ]
    },
    {
      "begin": "1737.400",
      "children": [],
      "end": "1737.440",
      "id": "f000184",
      "language": "hi",
      "lines": [
