    {
      "begin": "234.360",
      "children": [],
      "end": "254.000",
      "id": "f000027",
      "language": "hi",
      "lines": [
        "स किलाश्रममन्त्यमाश्रितो निवसन्नावसथे पुराद्बहिः । समुपास्यत पुत्रभोग्यया स्नुषयेवाविकृतेन्द्रियः श्रिया ॥"
      ]
    },
    {
      "begin": "254.000",
      "children": [],
      "end": "254.600",
      "id": "f000028",
      "language": "hi",
